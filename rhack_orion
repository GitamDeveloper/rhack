local Tab = Window:MakeTab({
	Name = "PROJECT_S",
	Icon = "",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "farm quest"
})

Tab:AddToggle({
	Name = "Sarah rice farm (105 quest)",
	Default = false,
	Callback = function(Value)
		project_s_ricefarm = Value
		if Value == true then
			project_s_removequest()
			while project_s_ricefarm == true and wait(0.1) do
				
				if game.Players.LocalPlayer.PlayerGui.Menu.Quest.Title.Text == "" then
					project_s_ricequesttake()
				else
					if game.Workspace:FindFirstChild("StarterVillage_RiceStrings"):FindFirstChild("RiceString") then
						if game.Players.LocalPlayer.PlayerGui.Menu.Quest.Title.Text == "" then
						else
							fireproximityprompt(game.Workspace:FindFirstChild("StarterVillage_RiceStrings"):FindFirstChild("RiceString"):FindFirstChild("Rice_pick_proximity"))
						end
					end
				end
			end
		end
	end    
})

Tab:AddToggle({
	Name = "Wagon deliver farm (150 quest)",
	Default = false,
	Callback = function(Value)
		project_s_wagonfarm = Value
		if Value == true then
			project_s_removequest()
			while project_s_wagonfarm == true and wait(0.1) do
				if game.Players.LocalPlayer.PlayerGui.Menu.Quest.Title.Text == "" then
					project_s_wagonquesttake()
				else
					if project_s_wagonfarm_tween == false then
						project_s_wagonfarm_tween = true
						project_s_moveto(CFrame.new(452.37561, 275.425903, -2671.28125, -0.998089015, 4.18701447e-08, -0.0617926493, 3.91806232e-08, 1, 4.4736634e-08, 0.0617926493, 4.22300701e-08, -0.998089015))
						project_s_wagonfarm_tween = false
					end
				end
			end
		end
	end    
})

local Section = Tab:AddSection({
	Name = "auto meditation"
})

Tab:AddToggle({
	Name = "auto meditation",
	Default = false,
	Callback = function(Value)
		project_s_auto_meditation = Value
		while project_s_auto_meditation == true and wait(0.1) do
			if game.Players.LocalPlayer.PlayerGui:FindFirstChild("ExcessGuis"):FindFirstChild("Meditate_gui") then
				game.Players.LocalPlayer.PlayerGui:FindFirstChild("ExcessGuis"):FindFirstChild("Meditate_gui"):FindFirstChild("Holder"):FindFirstChild("LocalScript"):FindFirstChild("Value").Value = 100
			else
				project_s_go_to_meditate()
			end
		end
	end    
})

