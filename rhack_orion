								wait(1)
							end
						else
							build_aboat_farmgold_count = 1
						end
						if players_gvar.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
							if build_aboat_farmgold_count == 0 then
								wait(1)
							end
						else
							build_aboat_farmgold_count = 1
						end
						if players_gvar.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
							if build_aboat_farmgold_count == 0 then
								wait(1)
							end
						else
							build_aboat_farmgold_count = 1
						end



						if players_gvar.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
							if build_aboat_farmgold_count == 0 then
								--players_gvar.LocalPlayer.Character.HumanoidRootPart.Anchored = false
							end
						else
							build_aboat_farmgold_count = 1
						end
					end
				end
			end
		end
	end    
})

Tab:AddDropdown({
	Name = "move mode",
	Default = "tp",
	Options = {"tp", "tween"},
	Callback = function(Value)
		if Value == "tp" then
			build_aboat_farmgold_type = "tp"
		end

		if Value == "tween" then
			build_aboat_farmgold_type = "tween"
		end
	end    
})

Tab:AddTextbox({
	Name = "move time",
	Default = "number",
	TextDisappear = true,
	Callback = function(Value)
		build_aboat_farmgold_movetime = tonumber(Value)
	end	  
})

local Section = Tab:AddSection({
	Name = "tower of hell"
})

Tab:AddToggle({
	Name = "auto farm money",
	Default = false,
	Callback = function(Value)
		towerhell_farmmoney = Value
		if Value == true then
			while towerhell_farmmoney == true and wait(0.1) do
				players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.tower.finishes.Finish.CFrame
				wait(5)
				if towerhell_farmmoney_rejoin == true then
					local ts = game:GetService("TeleportService")
					local p = game:GetService("Players").LocalPlayer
					ts:Teleport(game.PlaceId, p)
				end
			end
		end
	end    
})

Tab:AddToggle({
	Name = "rejoin",
	Default = false,
	Callback = function(Value)
		towerhell_farmmoney_rejoin = Value
	end    
})

local Section = Tab:AddSection({
	Name = "TSB"
})

Tab:AddButton({
	Name = "tp players",
	Callback = function()
		local pos = CFrame.new(0,0,0)
		pos = players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame

        local move = ""

		if players_gvar.LocalPlayer.Backpack:FindFirstChild("Flowing Water") then
			players_gvar.LocalPlayer.Backpack:FindFirstChild("Flowing Water").Parent = players_gvar.LocalPlayer.Character
		else
			if players_gvar.LocalPlayer.Backpack:FindFirstChild("Foul Ball") then
				players_gvar.LocalPlayer.Backpack:FindFirstChild("Foul Ball").Parent = players_gvar.LocalPlayer.Character
			else
				if players_gvar.LocalPlayer.Backpack:FindFirstChild("Atmos Cleave") then
					players_gvar.LocalPlayer.Backpack:FindFirstChild("Atmos Cleave").Parent = players_gvar.LocalPlayer.Character
				else
                    if players_gvar.LocalPlayer.Backpack:FindFirstChild("Crushing Pull") then
                        players_gvar.LocalPlayer.Backpack:FindFirstChild("Crushing Pull").Parent = players_gvar.LocalPlayer.Character
                    else
                        if players_gvar.LocalPlayer.Backpack:FindFirstChild("Head First") then
                            players_gvar.LocalPlayer.Backpack:FindFirstChild("Head First").Parent = players_gvar.LocalPlayer.Character
                        else
                            if players_gvar.LocalPlayer.Backpack:FindFirstChild("Freezing Path") then
                                players_gvar.LocalPlayer.Backpack:FindFirstChild("Freezing Path").Parent = players_gvar.LocalPlayer.Character
                                move = "Freezing Path"
                            else
                            end
                        end
                    end
				end
			end
		end

        wait(1)

		if tsb_tp_nick == "none" then
		else
			local pivot = players_gvar:FindFirstChild(tsb_tp_nick).Character:GetPivot()
			players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(pivot:PointToWorldSpace(Vector3.new(0, 0, 1)), pivot:PointToWorldSpace(Vector3.new(0, 0, 4)))
		end

		if players_gvar.LocalPlayer.Character:FindFirstChild("Flowing Water") then
			players_gvar.LocalPlayer.Character:FindFirstChild("Flowing Water").Parent = players_gvar.LocalPlayer.Backpack
		else
			if players_gvar.LocalPlayer.Character:FindFirstChild("Foul Ball") then
				players_gvar.LocalPlayer.Character:FindFirstChild("Foul Ball").Parent = players_gvar.LocalPlayer.Backpack
			else
				if players_gvar.LocalPlayer.Character:FindFirstChild("Atmos Cleave") then
					players_gvar.LocalPlayer.Character:FindFirstChild("Atmos Cleave").Parent = players_gvar.LocalPlayer.Backpack
				else
                    if players_gvar.LocalPlayer.Character:FindFirstChild("Crushing Pull") then
                        players_gvar.LocalPlayer.Character:FindFirstChild("Crushing Pull").Parent = players_gvar.LocalPlayer.Backpack
                    else
                        if players_gvar.LocalPlayer.Character:FindFirstChild("Head First") then
                            players_gvar.LocalPlayer.Character:FindFirstChild("Head First").Parent = players_gvar.LocalPlayer.Backpack
                        else
                            if players_gvar.LocalPlayer.Character:FindFirstChild("Freezing Path") then
                                players_gvar.LocalPlayer.Character:FindFirstChild("Freezing Path").Parent = players_gvar.LocalPlayer.Backpack
                            else
                            end
                        end
                    end
				end
			end
		end

		if tsb_tp_place == "air" then
			players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(100000,1000,100000)
		else
			if tsb_tp_place == "Death Cutscene" then
				players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Cutscenes:FindFirstChild("Death Cutscene").HeadCFrame.CFrame
			else
				if tsb_tp_place == "Atoms" then
					players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Cutscenes:FindFirstChild("Atoms").sphere.CFrame
				else
					if tsb_tp_place == "Map" then
						if game.Workspace.Thrown:FindFirstChild("Donation Leaderboard") then
							players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Thrown:FindFirstChild("Donation Leaderboard").Part.CFrame
						else
							if game.Workspace.Map:FindFirstChild("spone") then
								players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Map:FindFirstChild("spone").CFrame
							end
						end
					else
						if tsb_tp_place == "Map2" then
							players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Cutscenes:FindFirstChild("Atoms").ogcircle.CFrame
						else
							if tsb_tp_place == "Mountain" then

								if game.Workspace.Thrown:FindFirstChild("Donation Leaderboard") then
									players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(395.514648, 699.102539, 398.27771, -0.75630492, -9.94614453e-08, -0.65421927, -6.9310083e-08, 1, -7.19053759e-08, 0.65421927, -9.03839936e-09, -0.75630492)
								else
									if game.Workspace.Map:FindFirstChild("spone") then
										players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(490.736969, 688.797913, -362.606781, 0.826861322, -9.94782567e-09, -0.562405884, 7.48708917e-09, 1, -6.6803012e-09, 0.562405884, 1.31289934e-09, 0.826861322)
									end
								end
							else
                                if tsb_tp_place == "WaterMountain" then
                                    if game.Workspace.Thrown:FindFirstChild("Donation Leaderboard") then
                                        players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1174.80054, 816.735291, -810.939575, -0.622790992, 2.38599718e-08, 0.78238821, -3.14645945e-08, 1, -5.55425501e-08, -0.78238821, -5.92089293e-08, -0.622790992)
                                    end
                                end
                            end
						end
					end
				end
			end
		end

		if move == "Freezing Path" then
            print("move")
            wait(6)
        else
            wait(3)
        end

		if tsb_tp_back == true then
			players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
		end
	end    
})

