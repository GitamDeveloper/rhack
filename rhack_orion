local Section = Tab:AddSection({
	Name = "TSB"
})

Tab:AddButton({
	Name = "tp players",
	Callback = function()
		local pos = CFrame.new(0,0,0)
		pos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame

		if game.Players.LocalPlayer.Backpack:FindFirstChild("Flowing Water") then
			game.Players.LocalPlayer.Backpack:FindFirstChild("Flowing Water").Parent = game.Players.LocalPlayer.Character
		else
			if game.Players.LocalPlayer.Backpack:FindFirstChild("Foul Ball") then
				game.Players.LocalPlayer.Backpack:FindFirstChild("Foul Ball").Parent = game.Players.LocalPlayer.Character
			else
				if game.Players.LocalPlayer.Backpack:FindFirstChild("Atmos Cleave") then
					game.Players.LocalPlayer.Backpack:FindFirstChild("Atmos Cleave").Parent = game.Players.LocalPlayer.Character
				else
				end
			end
		end

		if tsb_tp_nick == "none" then
		else
			local pivot = game.Players:FindFirstChild(tsb_tp_nick).Character:GetPivot()
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(pivot:PointToWorldSpace(Vector3.new(0, 0, 1)), pivot:PointToWorldSpace(Vector3.new(0, 0, 4)))
		end
		wait(1)

		if game.Players.LocalPlayer.Character:FindFirstChild("Flowing Water") then
			game.Players.LocalPlayer.Character:FindFirstChild("Flowing Water").Parent = game.Players.LocalPlayer.Backpack
		else
			if game.Players.LocalPlayer.Character:FindFirstChild("Foul Ball") then
				game.Players.LocalPlayer.Character:FindFirstChild("Foul Ball").Parent = game.Players.LocalPlayer.Backpack
			else
				if game.Players.LocalPlayer.Character:FindFirstChild("Atmos Cleave") then
					game.Players.LocalPlayer.Character:FindFirstChild("Atmos Cleave").Parent = game.Players.LocalPlayer.Backpack
				else
				end
			end
		end

		if tsb_tp_place == "air" then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(100000,1000,100000)
		else
			if tsb_tp_place == "Death Cutscene" then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Cutscenes:FindFirstChild("Death Cutscene").HeadCFrame.CFrame
			else
				if tsb_tp_place == "Atoms" then
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Cutscenes:FindFirstChild("Atoms").sphere.CFrame
				else
					if tsb_tp_place == "Map" then
						if game.Workspace.Thrown:FindFirstChild("Donation Leaderboard") then
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Thrown:FindFirstChild("Donation Leaderboard").Part.CFrame
						else
							if game.Workspace.Map:FindFirstChild("spone") then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Map:FindFirstChild("spone").CFrame
							end
						end
					else
						if tsb_tp_place == "Map2" then
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Cutscenes:FindFirstChild("Atoms").ogcircle.CFrame
						else
							if tsb_tp_place == "Mountain" then

								if game.Workspace.Thrown:FindFirstChild("Donation Leaderboard") then
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(395.514648, 699.102539, 398.27771, -0.75630492, -9.94614453e-08, -0.65421927, -6.9310083e-08, 1, -7.19053759e-08, 0.65421927, -9.03839936e-09, -0.75630492)
								else
									if game.Workspace.Map:FindFirstChild("spone") then
										game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(490.736969, 688.797913, -362.606781, 0.826861322, -9.94782567e-09, -0.562405884, 7.48708917e-09, 1, -6.6803012e-09, 0.562405884, 1.31289934e-09, 0.826861322)
									end
								end
							end
						end
					end
				end
			end
		end

		wait(3)

		if tsb_tp_back == true then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
		end
	end    
})

Tab:AddBind({
	Name = "tp players",
	Default = Enum.KeyCode.F7,
	Hold = false,
	Callback = function()
		local pos = CFrame.new(0,0,0)
		pos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame

		if game.Players.LocalPlayer.Backpack:FindFirstChild("Flowing Water") then
			game.Players.LocalPlayer.Backpack:FindFirstChild("Flowing Water").Parent = game.Players.LocalPlayer.Character
		else
			if game.Players.LocalPlayer.Backpack:FindFirstChild("Foul Ball") then
				game.Players.LocalPlayer.Backpack:FindFirstChild("Foul Ball").Parent = game.Players.LocalPlayer.Character
			else
				if game.Players.LocalPlayer.Backpack:FindFirstChild("Atmos Cleave") then
					game.Players.LocalPlayer.Backpack:FindFirstChild("Atmos Cleave").Parent = game.Players.LocalPlayer.Character
				else
				end
			end
		end

		if tsb_tp_nick == "none" then
		else
			local pivot = game.Players:FindFirstChild(tsb_tp_nick).Character:GetPivot()
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(pivot:PointToWorldSpace(Vector3.new(0, 0, 1)), pivot:PointToWorldSpace(Vector3.new(0, 0, 4)))
		end
		wait(1)

		if game.Players.LocalPlayer.Character:FindFirstChild("Flowing Water") then
			game.Players.LocalPlayer.Character:FindFirstChild("Flowing Water").Parent = game.Players.LocalPlayer.Backpack
		else
			if game.Players.LocalPlayer.Character:FindFirstChild("Foul Ball") then
				game.Players.LocalPlayer.Character:FindFirstChild("Foul Ball").Parent = game.Players.LocalPlayer.Backpack
			else
				if game.Players.LocalPlayer.Character:FindFirstChild("Atmos Cleave") then
					game.Players.LocalPlayer.Character:FindFirstChild("Atmos Cleave").Parent = game.Players.LocalPlayer.Backpack
				else
				end
			end
		end

		if tsb_tp_place == "air" then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(100000,1000,100000)
		else
			if tsb_tp_place == "Death Cutscene" then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Cutscenes:FindFirstChild("Death Cutscene").HeadCFrame.CFrame
			else
				if tsb_tp_place == "Atoms" then
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Cutscenes:FindFirstChild("Atoms").sphere.CFrame
				else
					if tsb_tp_place == "Map" then
						if game.Workspace.Thrown:FindFirstChild("Donation Leaderboard") then
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Thrown:FindFirstChild("Donation Leaderboard").Part.CFrame
						else
							if game.Workspace.Map:FindFirstChild("spone") then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Map:FindFirstChild("spone").CFrame
							end
						end
					else
						if tsb_tp_place == "Map2" then
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Cutscenes:FindFirstChild("Atoms").ogcircle.CFrame
						else
							if tsb_tp_place == "Mountain" then

								if game.Workspace.Thrown:FindFirstChild("Donation Leaderboard") then
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(395.514648, 699.102539, 398.27771, -0.75630492, -9.94614453e-08, -0.65421927, -6.9310083e-08, 1, -7.19053759e-08, 0.65421927, -9.03839936e-09, -0.75630492)
								else
									if game.Workspace.Map:FindFirstChild("spone") then
										game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(490.736969, 688.797913, -362.606781, 0.826861322, -9.94782567e-09, -0.562405884, 7.48708917e-09, 1, -6.6803012e-09, 0.562405884, 1.31289934e-09, 0.826861322)
									end
								end
							end
						end
					end
				end
			end
		end

