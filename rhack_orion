Tab:AddButton({
	Name = "this is for dev (dont press)",
	Callback = function()
		for i,v in game.Workspace.bug_clones:GetDescendants() do
			v:Destroy()
		end


		players_gvar.LocalPlayer.Character:Clone().Parent = bug_clones
		players_gvar.LocalPlayer.Character.Parent = game.Workspace

		players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,2100,0)

		local speed = players_gvar.LocalPlayer.Character.Humanoid.WalkSpeed
		local jump = players_gvar.LocalPlayer.Character.Humanoid.JumpPower
		local Maxh = players_gvar.LocalPlayer.Character.Humanoid.MaxHealth
		local h = players_gvar.LocalPlayer.Character.Humanoid.Health

		game.Workspace.Camera.CameraSubject = game.Workspace.bug_clones:FindFirstChild(players_gvar.LocalPlayer.Name).Humanoid
		players_gvar.LocalPlayer.Character = game.Workspace.bug_clones:FindFirstChild(players_gvar.LocalPlayer.Name)

		game.Workspace.bug_clones:FindFirstChild(players_gvar.LocalPlayer.Name).Humanoid.WalkSpeed = speed
		game.Workspace.bug_clones:FindFirstChild(players_gvar.LocalPlayer.Name).Humanoid.JumpPower = jump
		game.Workspace.bug_clones:FindFirstChild(players_gvar.LocalPlayer.Name).Humanoid.MaxHealth = Maxh
		game.Workspace.bug_clones:FindFirstChild(players_gvar.LocalPlayer.Name).Humanoid.Health = h

		wait(1)

		print("hhh")
		game.Workspace:FindFirstChild(players_gvar.LocalPlayer.Name).HumanoidRootPart.CFrame = players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame
		players_gvar.LocalPlayer.Character = game.Workspace:FindFirstChild(players_gvar.LocalPlayer.Name)
		game.Workspace.Camera.CameraSubject = players_gvar.LocalPlayer.Character.Humanoid
		if game.Workspace.bug_clones:FindFirstChild(players_gvar.LocalPlayer.Name) then
			game.Workspace.bug_clones:FindFirstChild(players_gvar.LocalPlayer.Name):Destroy()
		end

		game.Workspace:FindFirstChild(players_gvar.LocalPlayer.Name).Parent = game.Workspace.bug_clones

		if game.Workspace:FindFirstChild(players_gvar.LocalPlayer.Name) then
			game.Workspace:FindFirstChild(players_gvar.LocalPlayer.Name):Destroy()
		end

	end    
})

