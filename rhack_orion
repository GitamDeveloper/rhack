			end
		end
	end    
})

local Tab = Window:MakeTab({
	Name = "KINGL",
	Icon = "",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "auto farm lvl"
})

function kingl_getquest()
	local lvl = game.Players.LocalPlayer.PlayerStats.lvl.Value
	if lvl < 10 then
		local args = {
    		[1] = "take",
    		[2] = "Kill 4 Soldiers"
		}

		game:GetService("ReplicatedStorage").Chest.Remotes.Functions.Quest:InvokeServer(unpack(args))
	elseif lvl < 20 then
		local args = {
    		[1] = "take",
    		[2] = "Kill 5 Clown Pirates"
		}

		game:GetService("ReplicatedStorage").Chest.Remotes.Functions.Quest:InvokeServer(unpack(args))
	elseif lvl < 30 then
		local args = {
    		[1] = "take",
    		[2] = "Kill 1 Smoky"
		}

		game:GetService("ReplicatedStorage").Chest.Remotes.Functions.Quest:InvokeServer(unpack(args))
	elseif lvl < 50 then
		local args = {
    		[1] = "take",
    		[2] = "Kill 1 Tashi"
		}

		game:GetService("ReplicatedStorage").Chest.Remotes.Functions.Quest:InvokeServer(unpack(args))
	elseif lvl < 75 then
		local args = {
    		[1] = "take",
    		[2] = "Kill 6 Clown Swordman"
		}

		game:GetService("ReplicatedStorage").Chest.Remotes.Functions.Quest:InvokeServer(unpack(args))
	elseif lvl < 100 then
		local args = {
    		[1] = "take",
    		[2] = "Kill 1 The Clown"
		}

		game:GetService("ReplicatedStorage").Chest.Remotes.Functions.Quest:InvokeServer(unpack(args))
	elseif lvl < 120 then
		local args = {
    		[1] = "take",
    		[2] = "Kill 4 Commander"
		}

		game:GetService("ReplicatedStorage").Chest.Remotes.Functions.Quest:InvokeServer(unpack(args))
	elseif lvl < 145 then
		local args = {
    		[1] = "take",
    		[2] = "Kill 1 Captain"
		}

		game:GetService("ReplicatedStorage").Chest.Remotes.Functions.Quest:InvokeServer(unpack(args))
	elseif lvl < 180 then
		local args = {
    		[1] = "take",
    		[2] = "Kill 1 The Barbaric"
		}

		game:GetService("ReplicatedStorage").Chest.Remotes.Functions.Quest:InvokeServer(unpack(args))
	elseif lvl < 200 then
		local args = {
    		[1] = "take",
    		[2] = "Kill 4 Fighter Fishmans"
		}

		game:GetService("ReplicatedStorage").Chest.Remotes.Functions.Quest:InvokeServer(unpack(args))
	elseif lvl < 230 then
		local args = {
    		[1] = "take",
    		[2] = "Kill 1 Karate Fishman"
		}

		game:GetService("ReplicatedStorage").Chest.Remotes.Functions.Quest:InvokeServer(unpack(args))
	elseif lvl < 260 then
		local args = {
    		[1] = "take",
    		[2] = "Kill 1 Shark Man"
		}

		game:GetService("ReplicatedStorage").Chest.Remotes.Functions.Quest:InvokeServer(unpack(args))
	end
end

function kingl_getenem_name()
	local lvl = game.Players.LocalPlayer.PlayerStats.lvl.Value
	if lvl < 10 then
		kingl_autof_enemname = "Soldier [Lv. 1]"
		kingl_autof_area = "Starter Island Quest"
		kingl_autof_isboss = false
		print("quest taked: " .. "| enemy - " .. kingl_autof_enemname .. " | area - " .. kingl_autof_area .. " |")
	elseif lvl < 20 then
		kingl_autof_enemname = "Clown Pirate [Lv. 10]"
		kingl_autof_area = "Starter Island Quest"
		kingl_autof_isboss = false
		print("quest taked: " .. "| enemy - " .. kingl_autof_enemname .. " | area - " .. kingl_autof_area .. " |")
	elseif lvl < 30 then
		kingl_autof_enemname = "Smoky [Lv. 20]"
		kingl_autof_area = "Starter Island Quest"
		kingl_autof_isboss = true
		print("quest taked: " .. "| enemy - " .. kingl_autof_enemname .. " | area - " .. kingl_autof_area .. " |")
	elseif lvl < 50 then
		kingl_autof_enemname = "Tashi [Lv. 30]"
		kingl_autof_area = "Starter Island Quest"
		kingl_autof_isboss = true
		print("quest taked: " .. "| enemy - " .. kingl_autof_enemname .. " | area - " .. kingl_autof_area .. " |")
	elseif lvl < 75 then
		kingl_autof_enemname = "Clown Swordman [Lv. 50]"
		kingl_autof_area = "Pirate Island Quest"
		kingl_autof_isboss = false
		print("quest taked: " .. "| enemy - " .. kingl_autof_enemname .. " | area - " .. kingl_autof_area .. " |")
	elseif lvl < 100 then
		kingl_autof_enemname = "The Clown [Lv. 75]"
		kingl_autof_area = "Pirate Island Quest"
		kingl_autof_isboss = true
		print("quest taked: " .. "| enemy - " .. kingl_autof_enemname .. " | area - " .. kingl_autof_area .. " |")
	elseif lvl < 120 then
		kingl_autof_enemname = "Commander [Lv. 100]"
		kingl_autof_area = "Soldier Town Quest"
		kingl_autof_isboss = false
		print("quest taked: " .. "| enemy - " .. kingl_autof_enemname .. " | area - " .. kingl_autof_area .. " |")
	elseif lvl < 145 then
		kingl_autof_enemname = "Captain [Lv. 120]"
		kingl_autof_area = "Soldier Town Quest"
		kingl_autof_isboss = true
		print("quest taked: " .. "| enemy - " .. kingl_autof_enemname .. " | area - " .. kingl_autof_area .. " |")
	elseif lvl < 180 then
		kingl_autof_enemname = "The Barbaric [Lv. 145]"
		kingl_autof_area = "Soldier Town Quest"
		kingl_autof_isboss = true
		print("quest taked: " .. "| enemy - " .. kingl_autof_enemname .. " | area - " .. kingl_autof_area .. " |")
	elseif lvl < 200 then
		kingl_autof_enemname = "Fighter Fishman [Lv. 180]"
		kingl_autof_area = "Shark Island Quest"
		kingl_autof_isboss = false
		print("quest taked: " .. "| enemy - " .. kingl_autof_enemname .. " | area - " .. kingl_autof_area .. " |")
	elseif lvl < 230 then
		kingl_autof_enemname = "Karate Fishman [Lv. 200]"
		kingl_autof_area = "Shark Island Quest"
		kingl_autof_isboss = true
		print("quest taked: " .. "| enemy - " .. kingl_autof_enemname .. " | area - " .. kingl_autof_area .. " |")
	elseif lvl < 260 then
		kingl_autof_enemname = "Shark Man [Lv. 230]"
		kingl_autof_area = "Shark Island Quest"
		kingl_autof_isboss = true
		print("quest taked: " .. "| enemy - " .. kingl_autof_enemname .. " | area - " .. kingl_autof_area .. " |")
	end
end

function kingl_attack()
	local tool_name = "FS_" .. kingl_autof_tool .. "_M1"
	local args = {
    	[1] = tool_name
	}

	game:GetService("ReplicatedStorage").Chest.Remotes.Functions.SkillAction:InvokeServer(unpack(args))
end

Tab:AddToggle({
	Name = "auto farm",
	Default = false,
	Callback = function(Value)
		kingl_autof = Value
		while kingl_autof == true and wait(0.1) do
			if game.Players.LocalPlayer.PlayerGui.MainGui.QuestBoard.Visible == false then
				kingl_getquest()
			else
				kingl_getenem_name()
				for i,v in next, game.Workspace.Monster:GetDescendants() do
					if kingl_autof_isboss == false then
						if game.Workspace.Monster.Mon:FindFirstChild(kingl_autof_enemname) then
							if v.Name == kingl_autof_enemname then
								if v:FindFirstChild("aceser") then
								else
									for i,v in next, game.Workspace.Monster.Mon:GetDescendants() do
										if v:FindFirstChild("aceser") then
											if v.Name == kingl_autof_enemname then
												if v:FindFirstChild("Humanoid").Health > 0 then
													local pivot = v:GetPivot()
													game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(pivot:PointToWorldSpace(Vector3.new(0, 0, kingl_autof_dist)), pivot:PointToWorldSpace(Vector3.new(0, 0, 4)))

													kingl_attack()
												else
													v:FindFirstChild("aceser"):Destroy()
													v:Destroy()
												end
											end
										else
											if v.Name == kingl_autof_enemname then
												local aceser = Instance.new("Part", game.Workspace.Monster.Mon:FindFirstChild(kingl_autof_enemname))
												aceser.CanCollide = false
												aceser.Transparency = 1
												aceser.Name = "aceser"
											end
										end
									end
								end
							end
						else
							local tppos = game.Workspace.AllNPC:FindFirstChild(kingl_autof_area).CFrame
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = tppos
						end
					else
						if game.Workspace.Monster.Boss:FindFirstChild(kingl_autof_enemname) then
							if v.Name == kingl_autof_enemname then
								for i,v in next, game.Workspace.Monster.Boss:GetDescendants() do
									if v.Name == kingl_autof_enemname then
										if v:FindFirstChild("Humanoid").Health > 0 then
											local pivot = v:GetPivot()
											game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(pivot:PointToWorldSpace(Vector3.new(0, 0, kingl_autof_dist)), pivot:PointToWorldSpace(Vector3.new(0, 0, 4)))

											kingl_attack()
										end
									end
								end
							end
						else
							local tppos = game.Workspace.AllNPC:FindFirstChild(kingl_autof_area).CFrame
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = tppos
						end
					end
				end
			end
		end
	end    
})

Tab:AddDropdown({
	Name = "farm_tool",
	Default = "null",
	Options = kingl_autof_tools,
	Callback = function(Value)
		kingl_autof_tool = Value
	end    
})

