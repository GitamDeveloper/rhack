local l_targ

local Section = Tab:AddSection({
	Name = "Hit boxes"
})

Tab:AddToggle({
	Name = "On_Off",
	Default = false,
	Callback = function(Value)
		if Value == true then
			hitb_oo = true
function hitb()
	while wait(0.24) do
		if hitb_oo == true then
			for i,v in next, game:GetService('Players'):GetPlayers() do
				if v.Name ~= game:GetService('Players').LocalPlayer.Name then
					if hitb_t == "root part" then
						if v.Character.Humanoid.Health >= 1 then
							if frend_nohit == true then
                				if v:IsFriendsWith(game.Players.LocalPlayer.UserId) then
                				else
                					if v.Character:FindFirstChild("HumanoidRootPart") then
                						v.Character.HumanoidRootPart.Size = Vector3.new(hitb_s,hitb_s,hitb_s)
										v.Character.HumanoidRootPart.Transparency = 0.7
										v.Character.HumanoidRootPart.Color = Color3.new(1, 0, 0.0156863)
										v.Character.HumanoidRootPart.Material = "Neon"
										v.Character.HumanoidRootPart.CanCollide = false
                					end
									if hitb_ff == true then
										if hitbox_findfirst_err == false then
											if v.Character:FindFirstChildOfClass("ForceField") or v:FindFirstChildOfClass("ForceField") then
												if v.Character:FindFirstChild("Head") then
													v.Character.Head.Size = Vector3.new(1,2,1)
													v.Character.Head.Color = Color3.new(0, 145, 255)
												end
											end
										end
									end
									if hitb_i == true then
										if v.Character:FindFirstChild("HumanoidRootPart") then
											v.Character.HumanoidRootPart.Transparency = 0.99
										end
									end
                				end
                			else
                				if v.Character:FindFirstChild("HumanoidRootPart") then
                					v.Character.HumanoidRootPart.Size = Vector3.new(hitb_s,hitb_s,hitb_s)
									v.Character.HumanoidRootPart.Transparency = 0.7
									v.Character.HumanoidRootPart.Color = Color3.new(1, 0, 0.0156863)
									v.Character.HumanoidRootPart.Material = "Neon"
									v.Character.HumanoidRootPart.CanCollide = false
								end
								if hitb_ff == true then
									if hitbox_findfirst_err == false then
										if v.Character:FindFirstChildOfClass("ForceField") or v:FindFirstChildOfClass("ForceField") then
											if v.Character:FindFirstChild("Head") then
												v.Character.Head.Size = Vector3.new(1,2,1)
												v.Character.Head.Color = Color3.new(0, 145, 255)
											end
										end
									end
								end
								if hitb_i == true then
									if v.Character:FindFirstChild("HumanoidRootPart") then
										v.Character.HumanoidRootPart.Transparency = 0.99
									end
								end
                			end
						else
							if v.Character:FindFirstChild("HumanoidRootPart") then
								v.Character.HumanoidRootPart.Size = Vector3.new(1,2,1)
								v.Character.HumanoidRootPart.Transparency = 1
							end
						end
					end
					if hitb_t == "head" then
						if v.Character.Humanoid.Health >= 1 then
							if frend_nohit == true then
                				if v:IsFriendsWith(game.Players.LocalPlayer.UserId) then
                				else
                					if v.Character:FindFirstChild("Head") then
                						if v.Character:FindFirstChild("HumanoidRootPart") then
                							v.Character.Head.Size = Vector3.new(hitb_s,hitb_s,hitb_s)
											v.Character.HumanoidRootPart.Transparency = 0.7
											v.Character.Head.Color = Color3.new(1, 0, 0.0156863)
											v.Character.Head.Material = "Neon"
											v.Character.Head.CanCollide = false

											v.Character.HumanoidRootPart.Size = Vector3.new(hitb_s / 2,hitb_s / 2,hitb_s / 2)
											v.Character.HumanoidRootPart.Transparency = 0.7
											v.Character.HumanoidRootPart.Color = Color3.new(1, 0, 0.0156863)
											v.Character.HumanoidRootPart.Material = "Neon"
											v.Character.HumanoidRootPart.CanCollide = false
                						end
                					end
									if hitb_ff == true then
										if hitbox_findfirst_err == false then
											if v.Character:FindFirstChildOfClass("ForceField") or v:FindFirstChildOfClass("ForceField") then
												if v.Character:FindFirstChild("Head") then
													v.Character.Head.Size = Vector3.new(1,2,1)
													v.Character.Head.Color = Color3.new(0, 145, 255)
												end
											end
										end
									end
									if hitb_i == true then
										if v.Character:FindFirstChild("Head") then
											if v.Character:FindFirstChild("HumanoidRootPart") then
												v.Character.HumanoidRootPart.Transparency = 0.99
												v.Character.Head.Transparency = 0.99
											end
										end
									end
                				end
                			else
                				if v.Character:FindFirstChild("HumanoidRootPart") then
                					if v.Character:FindFirstChild("Head") then
                						v.Character.Head.Size = Vector3.new(hitb_s,hitb_s,hitb_s)
										v.Character.HumanoidRootPart.Transparency = 0.7
										v.Character.Head.Color = Color3.new(1, 0, 0.0156863)
										v.Character.Head.Material = "Neon"
										v.Character.Head.CanCollide = false

										v.Character.HumanoidRootPart.Size = Vector3.new(hitb_s / 2,hitb_s / 2,hitb_s / 2)
										v.Character.HumanoidRootPart.Transparency = 0.7
										v.Character.HumanoidRootPart.Color = Color3.new(1, 0, 0.0156863)
										v.Character.HumanoidRootPart.Material = "Neon"
										v.Character.HumanoidRootPart.CanCollide = false
                					end
                				end
								if hitb_ff == true then
									if hitbox_findfirst_err == false then
										if v.Character:FindFirstChildOfClass("ForceField") or v:FindFirstChildOfClass("ForceField") then
											if v.Character:FindFirstChild("Head") then
												v.Character.Head.Size = Vector3.new(1,2,1)
												v.Character.Head.Color = Color3.new(0, 145, 255)
											end
										end
									end
								end
								if hitb_i == true then
									if v.Character:FindFirstChild("Head") then
										if v.Character:FindFirstChild("HumanoidRootPart") then
											v.Character.HumanoidRootPart.Transparency = 0.99
											v.Character.Head.Transparency = 0.99
										end
									end
								end
                			end
						else
							if v.Character:FindFirstChild("HumanoidRootPart") then
								if v.Character:FindFirstChild("Head") then
									v.Character.Head.Size = Vector3.new(1,2,1)
									v.Character.HumanoidRootPart.Transparency = 1
								end
							end
						end
					end
				end
			end
		end
	end
end
hitb()
		else
			hitb_oo = false
			for i,v in next, game:GetService('Players'):GetPlayers() do
                if v.Name ~= game:GetService('Players').LocalPlayer.Name then
                	if hitbox_findfirst_err == false then
                		if v.Character:FindFirstChild("HumanoidRootPart") then
                			if v.Character:FindFirstChild("Head") then
                				v.Character.HumanoidRootPart.Size = Vector3.new(1,2,1)	
