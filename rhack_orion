getgenv().fireproximityprompt=function(pp)
    local oldpos=players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame
    local oldenabled=pp.Enabled
    local oldhold=pp.HoldDuration
    local oldrlos=pp.RequiresLineOfSight
    pp.Enabled=true
    pp.HoldDuration=0
    pp.RequiresLineOfSight=false
    players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame=pp.Parent.CFrame
    wait(.23)
    pp:InputHoldBegin()
    task.wait()
    pp:InputHoldEnd()
    wait(.1)
    players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame=oldpos
    pp.Enabled=pp.Enabled
    pp.HoldDuration=pp.HoldDuration
    pp.RequiresLineOfSight=pp.RequiresLineOfSight
end

function fireproximitypromptV2(ProximityPrompt, Amount, Skip)
    assert(ProximityPrompt, "Argument #1 Missing or nil")
    assert(typeof(ProximityPrompt) == "Instance" and ProximityPrompt:IsA("ProximityPrompt"), "Attempted to fire a Value that is not a ProximityPrompt")

    local HoldDuration = ProximityPrompt.HoldDuration
    if Skip then
        ProximityPrompt.HoldDuration = 0
    end

    for i = 1, Amount or 1 do
        ProximityPrompt:InputHoldBegin()
        if Skip then
            local RunService = game:GetService("RunService")
            local Start = time()
            repeat
                RunService.Heartbeat:Wait(0.1)
            until time() - Start > HoldDuration
        end
        ProximityPrompt:InputHoldEnd()
    end
    ProximityPrompt.HoldDuration = HoldDuration
end

function apply_text(object, name, text, color)
	local gener = Instance.new("BillboardGui")
	gener.Name = name
	gener.Adornee = object
	gener.Size = UDim2.new(0, 200, 0, 50)
	gener.StudsOffset = Vector3.new(0, 2, 0)
	gener.AlwaysOnTop = true
	gener.Parent = object
	local label = Instance.new("TextLabel")
	label.Size = UDim2.new(1, 0, 1, 0)
	label.Text = text
	label.TextColor3 = color
	label.BackgroundTransparency = 1
	label.TextStrokeTransparency = 0
	label.TextScaled = true
	label.Parent = gener
end

function play_sound(sound_id, volume)
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://".. tostring(sound_id)
	sound.Parent = players_gvar.LocalPlayer.Character
	sound.Volume = volume
	sound:Play()
	--wait(sound.TimeLength)
	--sound:Destroy()
end

function apply_tracer(obj, color, size)

    local esp = Drawing.new("Line")
    esp.Visible = false
    esp.Center = true
    esp.Outline = true
    esp.Font = 2
    esp.Color = color
    esp.Size = size

    while wait(0.1) do
    	if obj then
            local obj_pos, obj_onscreen = camera:WorldToViewportPoint(obj.Position)

            if obj_onscreen then
            	esp.To = Vector2.new(obj_pos.X, obj_pos.Y)
            	esp.From = Vector2.new(camera.ViewPortSize.X / 2, camera.ViewportSize.Y /2)
                esp.Position = Vector2.new(obj_pos.X, obj_pos.Y)
                esp.Visible = true
            else 
                esp.Visible = false
            end
        else
            esp.Visible = false
            esp:Remove()
        end
    end
end

