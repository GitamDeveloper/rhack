Tab:AddButton({
	Name = "give item",
	Callback = function()
		if game.Workspace.Pickups:FindFirstChild(gef_giveitem) then
			fireproximityprompt(game.Workspace.Pickups:FindFirstChild(gef_giveitem).ProximityPrompt)
		else
			Library:MakeNotification({
				Name = "Fail!",
				Content = "give item function couldnt locate the item, because this item doesnt exist now. Please try later.",
				Image = "",
				Time = 10
			})
		end
  	end    
})

Tab:AddDropdown({
	Name = "item",
	Default = "none",
	Options = {"Hammer","Medkit","Lantern","Soda","Food","Bat","Crowbar","GPS","Handgun","Shotgun","Money","Bullets","Shells"},
