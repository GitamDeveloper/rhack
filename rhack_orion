local Library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local correct_key = "K1E2Y3B1Y2G3I4T5A5M6"
local iskey_correct = false

local disablekey_people = {"Alone_1755","doorslove1"}

local keyw = Library:MakeWindow({Name = "RHACK orion", HidePremium = true, SaveConfig = false, ConfigFolder = "rhack_orion", IntroEnabled = true, IntroText = "RHACK", IntroIcon = "rbxassetid://14995912169", Icon = "rbxassetid://14995912169"})

local keyt = keyw:MakeTab({
	Name = "Get Acces",
	Icon = "",
	PremiumOnly = false
})

local Section = keyt:AddSection({
	Name = "Key system"
})

keyt:AddTextbox({
	Name = "key",
	Default = "enter key",
	TextDisappear = true,
	Callback = function(Value)
		if Value == correct_key then
			iskey_correct = true
		else
			for i,v in disablekey_people do
				if v == game.Players.LocalPlayer.Name then
					iskey_correct = true
				end
			end
		end
	end	  
})

while iskey_correct == false and wait(0.1) do
	print("Enter Key In RHACK")
end


