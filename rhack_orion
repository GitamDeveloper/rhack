function project_s_moveto(cframe, event)
	local dist = (cframe.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
	local time = dist / 200

	local tween_goal = {CFrame = cframe}
	local tween_info = TweenInfo.new(time, Enum.EasingStyle.Linear)
	local tween = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"), tween_info, tween_goal)

	if project_s_tween_noclip == true then
		noclip()
	end
	tween:Play()
	wait(time)
	if project_s_tween_noclip == true then
		clip()
	end
end

function project_s_removequest()
	local args = {
    	[1] = "removequest",
    	[2] = game:GetService("ReplicatedStorage").Player_Data.scriptgDev3.Quest
	}

	game:GetService("ReplicatedStorage").Remotes.To_Server.Handle_Initiate_S:FireServer(unpack(args))
end

function project_s_ricequesttake()
	local args = {
    	[1] = "AddQuest",
    	[2] = "Players.scriptgDev3.PlayerGui.Npc_Dialogue.LocalScript.Functions",
    	[3] = 122556.271684684,
    	[4] = game:GetService("ReplicatedStorage").Player_Data:FindFirstChild(game.Players.LocalPlayer.Name).Quest,
    	[5] = {
        	["List"] = {
            	[1] = {
                	["Name"] = "pick 4 strings of rice",
                	["Progress"] = {
                    	[1] = 0,
                    	[2] = 4
                	}
            	}
        	},
        	["Current"] = "Help Sarah pick rice"
    	}
	}

	if project_s_ricequesttake_tween == false then
		project_s_ricequesttake_tween = true
		project_s_moveto(CFrame.new(-109.333763, 282.296783, -1697.01953, -0.602081895, -1.32820768e-08, -0.798434317, 8.54455062e-10, 1, -1.72794792e-08, 0.798434317, -1.10858878e-08, -0.602081895))
		project_s_ricequesttake_tween = false
	end
	game:GetService("ReplicatedStorage").Remotes.To_Server.Handle_Initiate_S:FireServer(unpack(args))
end

function project_s_wagonquesttake()
	local args = {
    	[1] = "AddQuest",
    	[2] = "Players.scriptgDev3.PlayerGui.Npc_Dialogue.LocalScript.Functions",
    	[3] = 131263.249807559,
    	[4] = game:GetService("ReplicatedStorage").Player_Data:FindFirstChild(game.Players.LocalPlayer.Name).Quest,
    	[5] = {
       		["List"] = {
            	[1] = {
                	["Name"] = "Deliver wagon",
                	["Progress"] = {
                    	[1] = 0,
                    	[2] = 1
                	}
            	}
        	},
        	["Current"] = "Deliver grandpa Wagwon's wagon"
    	}
	}

	
	if project_s_wagonquesttake_tween == false then
		project_s_wagonquesttake_tween = true
		project_s_moveto(CFrame.new(223.232788, 283.25769, -1605.61755, 0.707134247, 2.56967461e-08, 0.707079291, -5.31856372e-08, 1, 1.68476717e-08, -0.707079291, -4.9520029e-08, 0.707134247))
		project_s_wagonquesttake_tween = false
	end
	game:GetService("ReplicatedStorage").Remotes.To_Server.Handle_Initiate_S:FireServer(unpack(args))
end

