	if obj.Name == "Eyefestation" then
		if pressure_no_eye == true then
			task.wait(0.1)
			obj:Destroy()
		end
	end

	if obj.Name == "Steams" then
		if pressure_no_steams == true then
			task.wait(0.1)
			obj:Destroy()
		end
	end

	if obj.Name == "SearchlightsEncounter" then
		if pressure_no_SearchLights == true then
			task.wait(0.1)
			obj:Destroy()
		end
	end
end)

