local invs_clones = Instance.new("Folder",game.Workspace)
invs_clones.Name = "invs_clones"

Tab:AddBind({
	Name = "invisible and god mode",
	Default = Enum.KeyCode.F7,
	Hold = false,
	Callback = function()
		if tsb_invisible == false then
			tsb_invisible = true

			players_gvar.LocalPlayer.Character:Clone().Parent = game.Workspace.invs_clones
			players_gvar.LocalPlayer.Character.Parent = game.Workspace

			local char = game.Workspace.invs_clones:FindFirstChild(players_gvar.LocalPlayer.Name)

			if invs_mode == "tp" then
				players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,2100,0)
			else
				if invs_mode == "tween" then
					local tween_goal = {CFrame = CFrame.new(0,2100,0)}
					local tween_info = TweenInfo.new(invs_time, Enum.EasingStyle.Linear)
					local tween = game:GetService("TweenService"):Create(players_gvar.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"), tween_info, tween_goal)

					tween:Play()
				end
			end

			local speed = players_gvar.LocalPlayer.Character.Humanoid.WalkSpeed
			local jump = players_gvar.LocalPlayer.Character.Humanoid.JumpPower
			local Maxh = players_gvar.LocalPlayer.Character.Humanoid.MaxHealth
			local h = players_gvar.LocalPlayer.Character.Humanoid.Health

			game.Workspace.Camera.CameraSubject = char.Humanoid
			players_gvar.LocalPlayer.Character = char

			char.Humanoid.WalkSpeed = speed
			char.Humanoid.JumpPower = jump
			char.Humanoid.MaxHealth = Maxh
			char.Humanoid.Health = h
		else
			tsb_invisible = false

			if invs_mode == "tp" then
				game.Workspace:FindFirstChild(players_gvar.LocalPlayer.Name).HumanoidRootPart.CFrame = players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame
			else
				if invs_mode == "tween" then
					local tween_goal = {CFrame = players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame}
					local tween_info = TweenInfo.new(invs_time, Enum.EasingStyle.Linear)
					local tween = game:GetService("TweenService"):Create(game.Workspace:FindFirstChild(players_gvar.LocalPlayer.Name).HumanoidRootPart.CFrame, tween_info, tween_goal)

					tween:Play()
				end
			end

			players_gvar.LocalPlayer.Character = game.Workspace:FindFirstChild(players_gvar.LocalPlayer.Name)
			game.Workspace.Camera.CameraSubject = players_gvar.LocalPlayer.Character.Humanoid
			if game.Workspace.invs_clones:FindFirstChild(players_gvar.LocalPlayer.Name) then
				game.Workspace.invs_clones:FindFirstChild(players_gvar.LocalPlayer.Name):Destroy()
			end

			game.Workspace:FindFirstChild(players_gvar.LocalPlayer.Name).Parent = game.Workspace.invs_clones

			if game.Workspace:FindFirstChild(players_gvar.LocalPlayer.Name) then
				game.Workspace:FindFirstChild(players_gvar.LocalPlayer.Name):Destroy()
			end
		end
	end    
})

Tab:AddDropdown({
	Name = "move mode",
	Default = "tp",
	Options = {"tp", "tween"},
	Callback = function(Value)
		invs_mode = Value
	end    
})

Tab:AddTextbox({
	Name = "tween time",
	Default = "number",
	TextDisappear = true,
	Callback = function(Value)
		invs_time = tonumber(Value)
	end	  
})

