local Section = Tab:AddSection({
	Name = "auto farm lvl"
})

function tpto_bf(cframe)
	local dist = (cframe.Position - players_gvar.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
	local tw_speed = 200
	players_gvar.LocalPlayer.Character.HumanoidRootPart.Anchored = false
	if dist < 100 then
		players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = cframe
		tw_speed = 350
	elseif dist < 1000 then
		tw_speed = 350
	elseif dist >= 1000 then
		tw_speed = 300
	end

	local tw_pos = {CFrame = cframe}
	local tw_info = TweenInfo.new(dist/tw_speed, Enum.EasingStyle.Linear)
	local tween = game:GetService("TweenService"):Create(players_gvar.LocalPlayer.Character.HumanoidRootPart, tw_info, tw_pos)
	tweenend_time = dist/tw_speed
	tween:Play()

end

function getto_closest(object, search_name, esp, esp_color)
	local nearby
	local nearby_dist

	for i,v in next, object:GetDescendants() do
		if v.Name == search_name then
			local dist = (v.Position - players_gvar.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude

			if nearby then
				if nearby_dist > dist then
					nearby_dist = dist
					nearby = v
				end
			else
				nearby = object:FindFirstChild(search_name)
				nearby_dist = (object:FindFirstChild(search_name).Position - players_gvar.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
			end

		end
	end

	tpto_bf(nearby.CFrame)
	if esp == true then
		local espp = Instance.new("Highlight",nearby)
	    espp.FillColor = esp_color
        espp.Name = "ProccEsp"
	end
	
end

function Quest_lvl()
	local lvl = players_gvar.LocalPlayer.Data.Level.Value
	if bf_autof_skip_bos_quest == true then
		if lvl < 10 then
			questg = "Bandit Quest Giver"
			questpos = CFrame.new(10,10,10)
			questnum = 1
			questname = "BanditQuest1"
			enem_name = "Bandit"
		elseif lvl < 15 then
			questg = "Adventurer"
			questpos = CFrame.new(-1598.09, 35.5501, 153.378)
			questnum = 1
			questname = "JungleQuest"
			enem_name = "Monkey"
		elseif lvl >= 20 then
			questg = "Adventurer"
			questpos = CFrame.new(-1598.09, 35.5501, 153.378)
			questnum = 2
			questname = "JungleQuest"
			enem_name = "Gorilla"
		end
	else
		if lvl < 10 then
			questg = "Bandit Quest Giver"
			questpos = CFrame.new(10,10,10)
			questnum = 1
			questname = "BanditQuest1"
			enem_name = "Bandit"
		elseif lvl < 15 then
			questg = "Adventurer"
			questpos = CFrame.new(-1598.09, 35.5501, 153.378)
			questnum = 1
			questname = "JungleQuest"
			enem_name = "Monkey"
		elseif lvl < 20 then
			questg = "Adventurer"
			questpos = CFrame.new(-1598.09, 35.5501, 153.378)
			questnum = 2
			questname = "JungleQuest"
			enem_name = "Gorilla"
		elseif lvl >= 20 then
			questg = "Adventurer"
			questpos = CFrame.new(-1598.09, 35.5501, 153.378)
			questnum = 3
			questname = "JungleQuest"
			enem_name = "The Gorilla King"
		end
	end
end

Tab:AddToggle({
	Name = "auto farm",
	Default = false,
	Callback = function(Value)
		if gravity_err == false then
			game.Workspace.Gravity = 100
		end
		players_gvar.LocalPlayer.Character.HumanoidRootPart.Anchored = false
		bf_autof = Value
		while bf_autof == true and wait(0.1) do
			if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
				Quest_lvl()
				if game.Workspace.NPCs:FindFirstChild(questg) then
					tpto_bf(game.Workspace.NPCs:FindFirstChild(questg).HumanoidRootPart.CFrame)
					if bf_autof_esp == true then
						local espp = Instance.new("Highlight",game.Workspace.NPCs:FindFirstChild(questg))
	    				espp.FillColor = Color3.fromRGB(75, 151, 75)
        				espp.Name = "ProccEsp"
					end

        			wait(tweenend_time)

        			if bf_autof_esp == true then
        				espp.Enabled = false
        			end
				else
					tpto_bf(questpos)

					wait(tweenend_time)
				end
				game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest",questname,questnum)
			else
				if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true then
					for i,v in next, game.Workspace.Enemies:GetDescendants() do
						if game.Workspace.Enemies:FindFirstChild(enem_name) then
							if v.Name == enem_name then
								if v:FindFirstChild("HumanoidRootPart") then
									if v:FindFirstChild("Humanoid") then
										if v:FindFirstChild("Humanoid").Health > 0 then
											if v:FindFirstChild("aceser") then
											else
												for i,v in next, game.Workspace.Enemies:GetDescendants() do
													if v:FindFirstChild("aceser") then
														if v.Name == enem_name then
															if v:FindFirstChild("Humanoid").Health > 0 then
																if gravity_err == false then
																	game.Workspace.Gravity = 0
																end

																v.HumanoidRootPart.Size = Vector3.new(30,30,30)
																v.HumanoidRootPart.CanCollide = false
																v.HumanoidRootPart.Transparency = 1

																if bf_autof_coll == true then
																	local dist = (v.HumanoidRootPart.Position - players_gvar.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
																	if dist <= bf_autof_coll_range then
																		for i,v in next, game.Workspace.Enemies:GetDescendants() do
																			if v.Name == enem_name then
																				v.HumanoidRootPart.CFrame = players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,-20,0)
																			end
																		end
																	end
																end

																if players_gvar.LocalPlayer.Character.HumanoidRootPart.Position == v.HumanoidRootPart.Position then
																else
																	tpto_bf(v.HumanoidRootPart.CFrame * CFrame.new(0,20,0))

																	if bf_autof_esp == true then
																		local espp = Instance.new("Highlight",v)
	    																espp.FillColor = Color3.fromRGB(196, 40, 28)
        																espp.Name = "ProccEsp"
																	end

																	wait(tweenend_time)

																	if bf_autof_esp == true then
																		espp.Enabled = false
																	end
																	players_gvar.LocalPlayer.Character.HumanoidRootPart.Anchored = true
																end

																if players_gvar.LocalPlayer.Character:FindFirstChild(bf_autof_tool) then
																else

																players_gvar.LocalPlayer.Backpack:FindFirstChild(bf_autof_tool).Parent = players_gvar.LocalPlayer.Character
																local args = {
    																[1] = "weaponChange",
    																[2] = bf_autof_tool
																}

																game:GetService("ReplicatedStorage").RigControllerEvent:FireServer(unpack(args))

																end
																game:GetService"VirtualUser":CaptureController()
			    												game:GetService"VirtualUser":Button1Down(Vector2.new(0,1,0,1))
			    												--print(bf_autof)
															else
																v:FindFirstChild("aceser"):Destroy()
																v:Destroy()
															end
														end
													else
														if v.Name == enem_name then
															local aceser = Instance.new("Part",game.Workspace.Enemies:FindFirstChild(enem_name))
															aceser.CanCollide = false
															aceser.Transparency = 1
															aceser.Name = "aceser"
														end
													end
												end
											end
										else
											v.HumanoidRootPart.Size = Vector3.new(1,1,1)
											v.HumanoidRootPart.CanCollide = false
											v.HumanoidRootPart.Transparency = 0.3
										end
									end
								end
							end
						else
							local enem_spawn = game.Workspace._WorldOrigin.EnemySpawns:FindFirstChild(enem_name)
							if players_gvar.LocalPlayer.Character.HumanoidRootPart.Position == enem_spawn.Position then
							else
								tpto_bf(enem_spawn.CFrame * CFrame.new(0,20,0))
								wait(tweenend_time)
								players_gvar.LocalPlayer.Character.HumanoidRootPart.Anchored = true
							end
						end
					end
				end
			end
		end
	end    
})

