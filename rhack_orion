local hbt_p

Tab:AddToggle({
	Name = "teleport hitbox",
	Default = false,
	Callback = function(Value)
		if Value == true then
			hitb_tp = true
			hbt_p = eplayer.Character.HumanoidRootPart.CFrame
			while hitb_tp == true and wait(0.24) do
            	for i,v in next, game:GetService("Players"):GetPlayers() do
                	if v.Name ~= game:GetService("Players").LocalPlayer.Name then
                		if frend_nohit == true then
                			if v:IsFriendsWith(game.Players.LocalPlayer.UserId) then
                			else
                				v.Character.HumanoidRootPart.Size = Vector3.new(10,10,10)
                    			v.Character.HumanoidRootPart.CFrame = hbt_p
                    			v.Character.HumanoidRootPart.CanCollide = false
                			end
                		else
                			v.Character.HumanoidRootPart.Size = Vector3.new(10,10,10)
                    		v.Character.HumanoidRootPart.CFrame = hbt_p
                    		v.Character.HumanoidRootPart.CanCollide = false
                		end
                	end
            	end
        	end
		else
			hitb_tp = false
		end
	end    
})

