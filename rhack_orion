local Section = Tab:AddSection({
	Name = "esp"
})

function create_new_espbox()
    local box = Drawing.new("Square")
    box.Thickness = 2
    box.Filled = false
    box.Size = Vector2.new(0,0)
    box.Visible = false

    return box
end

local players_box = {}

Tab:AddToggle({
	Name = "Esp (players)",
	Default = false,
	Callback = function(Value)
		esp_onoff = Value

		if Value == true then
            
			while esp_onoff == true and wait() do
            	for i, v in pairs(workspace:GetDescendants()) do
            		if v.Name == "TextEsp" then
            			if v.Adornee == nil then
            				v.Destroy()
            			end
            		end

                	if v:FindFirstChild("Humanoid") and players_gvar:FindFirstChild(v.Name) then
                		if esp_type == "highlight" then
                			if not v:FindFirstChild("highEsp") then
                				if players_gvar.LocalPlayer.Character then
                					if v ~= players_gvar.LocalPlayer.Character then

                            			if esp_outline_only == true then
                            				local esp = Instance.new("Highlight",v)
	                            			esp.FillTransparency = 1
	                            			esp.OutlineColor = esp_colorpick
                            				esp.Name = "highEsp"
                            			else
                            				local esp = Instance.new("Highlight",v)
	                            			esp.FillColor = esp_colorpick
	                            			esp.OutlineColor = esp_colorpick
                            				esp.Name = "highEsp"
                            			end
                        			end
                				end
                    		end
                		else
                			if esp_type == "text" then
                				if not v:FindFirstChild("TextEsp") then
                					if players_gvar.LocalPlayer.Character then
                						if v ~= players_gvar.LocalPlayer.Character then
                							apply_text(v, "TextEsp", v.Name, esp_colorpick)
                						end
                					end
                				end
                			else
                                if esp_type == "2d" then
                                    if players_gvar.LocalPlayer.Character then
                                        if v ~= players_gvar.LocalPlayer.Character then


                                            local find_in_plrbox = false
                                            local box = nil

                                            for o,m in ipairs(players_box) do
                                                if m[1] and m[2] then
                                                    if m[1] == v.Name then
                                                        box = m[2]
                                                        find_in_plrbox = true

                                                        print("find box")
                                                    end
                                                end
                                            end

                                            if find_in_plrbox == false then
                                                local to_insert = {v.Name,create_new_espbox()}

                                                table.insert(players_box,to_insert)

                                                box = to_insert[2]

                                                print("created new box")
                                            end

                                            if box ~= nil then

                                                print("moving box")
                                                local player_scr_pos = camera:WorldToScreenPoint(v.HumanoidRootPart.Position)

                                                local pos_dif = (players_gvar.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude

                                                if pos_dif == 0 then
                                                    pos_dif = 1
                                                end

                                                if pos_dif < 0 then
                                                    pos_dif = pos_dif * -1
                                                end

                                                local box_pos = Vector2.new(player_scr_pos.X,player_scr_pos.Y)

                                                local box_size_x = box_pos.X - (pos_dif / 10)
                                                local box_size_y = box_pos.Y - (pos_dif / 10)

                                                local box_size = box_pos - Vector2.new((pos_dif ),(pos_dif))

                                                box.Position = box_pos
                                                box.Size = box_size
                                                box.Visible = true
                                            end
                                        end
                                    end
                                end
                            end
                		end
                	end
            	end
        	end
        else
        end
	end    
})

Tab:AddDropdown({
	Name = "type",
	Default = "highlight",
	Options = {"text", "highlight","2d"},
	Callback = function(Value)
		esp_type = Value
	end    
