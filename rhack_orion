Tab:AddToggle({
	Name = "auto farm gold",
	Default = false,
	Callback = function(Value)
		if Value == true then
			build_aboat_farmgold = Value
			if build_aboat_farmgold_type == "tp" then
				while build_aboat_farmgold == true and wait(0.1) do
					if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
						build_aboat_farmgold_count = 0
						if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
							if build_aboat_farmgold_count == 0 then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.BoatStages.NormalStages.CaveStage1.StonePart.CFrame
							end
						else
							build_aboat_farmgold_count = 1
						end

						wait(build_aboat_farmgold_time)

						if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
							if build_aboat_farmgold_count == 0 then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.BoatStages.NormalStages.CaveStage2.StonePart.CFrame
							end
						else
							build_aboat_farmgold_count = 1
						end

						wait(build_aboat_farmgold_time)

						if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
							if build_aboat_farmgold_count == 0 then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.BoatStages.NormalStages.CaveStage3.StonePart.CFrame
							end
						else
							build_aboat_farmgold_count = 1
						end

						wait(build_aboat_farmgold_time)

						if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
							if build_aboat_farmgold_count == 0 then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.BoatStages.NormalStages.CaveStage4.StonePart.CFrame
							end
						else
							build_aboat_farmgold_count = 1
						end

						wait(build_aboat_farmgold_time)

						if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
							if build_aboat_farmgold_count == 0 then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.BoatStages.NormalStages.CaveStage5.StonePart.CFrame
							end
						else
							build_aboat_farmgold_count = 1
						end

						wait(build_aboat_farmgold_time)

						if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
							if build_aboat_farmgold_count == 0 then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.BoatStages.NormalStages.CaveStage6.StonePart.CFrame
							end
						else
							build_aboat_farmgold_count = 1
						end

						wait(build_aboat_farmgold_time)

						if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
							if build_aboat_farmgold_count == 0 then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.BoatStages.NormalStages.CaveStage7.StonePart.CFrame
							end
						else
							build_aboat_farmgold_count = 1
						end

						wait(build_aboat_farmgold_time)

						if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
							if build_aboat_farmgold_count == 0 then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.BoatStages.NormalStages.CaveStage8.StonePart.CFrame
							end
						else
							build_aboat_farmgold_count = 1
						end

						wait(build_aboat_farmgold_time)

						if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
							if build_aboat_farmgold_count == 0 then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.BoatStages.NormalStages.CaveStage9.StonePart.CFrame
							end
						else
							build_aboat_farmgold_count = 1
						end

						wait(build_aboat_farmgold_time)

						if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
							if build_aboat_farmgold_count == 0 then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.BoatStages.NormalStages.CaveStage10.StonePart.CFrame
							end
						else
							build_aboat_farmgold_count = 1
						end

						wait(build_aboat_farmgold_time)

						if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
							if build_aboat_farmgold_count == 0 then
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.BoatStages.NormalStages.TheEnd.GoldenChest.Trigger.CFrame
							end
						else
							build_aboat_farmgold_count = 1
						end
						wait(1)
					end
				end
			else
				if build_aboat_farmgold_type == "tween" then
					build_aboat_farmgold_count = 0
					if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
						if build_aboat_farmgold_count == 0 then
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.BoatStages.NormalStages.CaveStage1.StonePart.CFrame
						end
					else
						build_aboat_farmgold_count = 1
					end

					local tween_to_end1_goal = {Position = workspace.BoatStages.NormalStages.CaveStage10.StonePart.Position}
					local tween_to_end1_info = TweenInfo.new(5)
					local tween_to_end1 = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, tween_to_end1_info, tween_to_end1_goal)

					tween_to_end1:Play()

					wait(5)

					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.BoatStages.NormalStages.TheEnd.GoldenChest.Trigger.CFrame
				end
			end
		end
	end    
})

