Tab:AddToggle({
    Name = "auto frozen lock",
    Default = false,
    Callback = function(Value)
        tsb_auto_frozenlock = Value
        while wait() and tsb_auto_frozenlock == true  do
            if game.Workspace.Thrown:FindFirstChild("Frozen Lock") then
                players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Thrown:FindFirstChild("Frozen Lock").Cube.CFrame
                tsb_auto_frozenlock = false
            end
        end
    end    
})

local tsb_tp_target_plr = nil

Tab:AddToggle({
    Name = "target plr",
    Default = false,
    Callback = function(Value)
        tsb_tp_target = Value

        while tsb_tp_target == true and wait() do
            players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = tsb_tp_target_plr.Character.HumanoidRootPart.CFrame
        end
    end    
})

Tab:AddTextbox({
    Name = "plr",
    Default = "username",
    TextDisappear = true,
    Callback = function(Value)
        if players_gvar:FindFirstChild(Value) then
            tsb_tp_target_plr = players_gvar:FindFirstChild(Value)
        end
    end   
})



game:GetService("UserInputService").InputBegan:Connect(function(input, isProcessed)
	if tsb_autobug1 == true then
		if input.KeyCode == Enum.KeyCode.Two then

			if players_gvar.LocalPlayer.Backpack:FindFirstChild("Atmos Cleave") then
				players_gvar.LocalPlayer.Backpack:FindFirstChild("Atmos Cleave").Parent = players_gvar.LocalPlayer.Character
			else
			end

			wait(1)
			local ts = game:GetService("TeleportService")
			local p = game:GetService("Players").LocalPlayer
			ts:TeleportToPlaceInstance(game.PlaceId, game.JobId, p)

			if players_gvar.LocalPlayer.Character:FindFirstChild("Atmos Cleave") then
				players_gvar.LocalPlayer.Character:FindFirstChild("Atmos Cleave").Parent = players_gvar.LocalPlayer.Backpack
			else
			end

        end
	end
end)

local Section = Tab:AddSection({
	Name = "UBG"
})

Tab:AddButton({
	Name = "give fake MOB",
	Callback = function()
		players_gvar.LocalPlayer.Data:FindFirstChild("Character").Value = "Mob"
	end    
})

game:GetService("UserInputService").InputBegan:Connect(function(input, isProcessed)
	if ubg_fling == true then
		if input.KeyCode == Enum.KeyCode.One then
			if players_gvar.LocalPlayer.Data:FindFirstChild("Character").Value == "Gon" then
				local pos = CFrame.new()
				pos = players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame

				if ubg_fling_place == "air" then
					local fling_goal = {CFrame = CFrame.new(1000,1000,1000)}
					local fling_info = TweenInfo.new(1, Enum.EasingStyle.Linear)
					local fling = game:GetService("TweenService"):Create(players_gvar.LocalPlayer.Character.HumanoidRootPart, fling_info, fling_goal)

					wait(1)

					fling:Play()
				else
					if ubg_fling_place == "castle" then
						local fling_goal = {CFrame = CFrame.new(586.419067, 120.410744, -499.891113, -3.6525809e-14, -4.33599396e-08, -1, -3.07723127e-08, 1, -4.33599396e-08, 1, 3.07723127e-08, -3.78600925e-14)}
						local fling_info = TweenInfo.new(1, Enum.EasingStyle.Linear)
						local fling = game:GetService("TweenService"):Create(players_gvar.LocalPlayer.Character.HumanoidRootPart, fling_info, fling_goal)

						wait(1)

						fling:Play()
					end
				end

				wait(3)

				if ubg_fling_back == true then
					local back_goal = {CFrame = pos}
					local back_info = TweenInfo.new(2, Enum.EasingStyle.Linear)
					local back = game:GetService("TweenService"):Create(players_gvar.LocalPlayer.Character.HumanoidRootPart, back_info, back_goal)

					back:Play()
				end
			end
		end
	end
end)

game:GetService("UserInputService").InputBegan:Connect(function(input, isProcessed)
	if ubg_fling == true then
		if input.KeyCode == Enum.KeyCode.Three then
			if players_gvar.LocalPlayer.Data:FindFirstChild("Character").Value == "Gon" then
				local pos = CFrame.new()
				pos = players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame

