local Tab = Window:MakeTab({
    Name = "DEVAS",
    Icon = "",
    PremiumOnly = false
})

local Section = Tab:AddSection({
    Name = "Auto Kill"
})

Tab:AddToggle({
    Name = "On/Off",
    Default = false,
    Callback = function(Value)
        devas_autokill = Value
        while wait() and devas_autokill == true do
            for i,v in game.Workspace.Game.Regions.Dion.Areas.AncientRuins.MobsSpots:GetChildren() do
                local acces1 = false
                for i,b in devas_autokill_list do
                    if v.Name == b[2] then
                        acces1 = true
                    end
                end

                if acces1 == true then

                    for i,n in v:GetChildren() do
                        local acces2 = false
                        local uuid = ""
                        for i,m in devas_autokill_list do
                            if n.Name == m[2] then
                                acces2 = true
                                uuid = m[1]
                            end
                        end

                        if acces2 == true then
                            local Distance = (n.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude

                            if Distance <= devas_autokill_range then
                                local acces3 = false

                                for i,j in n:GetDescendants() do
                                    if j.ClassName == "Humanoid" then
                                        if j.Health >= 1 then
                                            acces3 = true
                                        end
                                    end
                                end

                                if acces3 == true then
                                    if devas_autokill_weapon == "Sword" then

                                        local args = {
                                            [1] = {
                                                [uuid] = {
                                                    ["clientAuthoroty"] = true,
                                                    ["additionalData"] = {
                                                        ["hittedPartName"] = ""
                                                    },
                                                    ["aimedTarget"] = uuid
                                                }
                                            }
                                        }

                                        game:GetService("ReplicatedStorage").Events.Combat.MeleeWeaponAttackHit:FireServer(unpack(args))

                                    else
                                        if devas_autokill_weapon == "Bow" then

                                            local args1 = {
                                                [1] = 1,
                                                [2] = "rhack_gitam",
                                                [3] = game.Players.LocalPlayer.Character.HumanoidRootPart.Position,
                                                [4] = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                                            }

                                            game:GetService("ReplicatedStorage").Events.Combat.RangeWeaponAttack:FireServer(unpack(args1))



                                            local args2 = {
                                                [1] = "rhack_gitam",
                                                [2] = uuid,
                                                [3] = {
                                                    ["additionalData"] = {
                                                        ["targetsHitData"] = {
                                                            [uuid] = {
                                                                ["isCriticalDamage"] = true,
                                                                ["targetHRP"] = nil,
                                                                ["damage"] = 1
                                                            }
                                                        },
                                                        ["hittedPartName"] = "",
                                                        ["hitPosition"] = game.Players.LocalPlayer.Character.HumanoidRootPart.Position,
                                                        ["attackId"] = "made_by_gitam"
                                                    },
                                                    ["casterId"] = game.Players.LocalPlayer.UserId,
                                                    ["weaponType"] = "Bow"
                                                }
                                            }

                                            game:GetService("ReplicatedStorage").Events.Combat.RangeWeaponAttackHit:FireServer(unpack(args2))
    
                                        end
                                    end

                                    wait(devas_autokill_delay)
                                else

                                end
                            end
                        end
                    end
                end
            end
        end
    end    
})

Tab:AddTextbox({
    Name = "Range",
    Default = "enter killing range",
    TextDisappear = true,
    Callback = function(Value)
        devas_autokill_range = tonumber(Value)
    end   
})

Tab:AddTextbox({
    Name = "Delay",
    Default = "enter attack delay",
    TextDisappear = true,
    Callback = function(Value)
        devas_autokill_delay = tonumber(Value)
    end   
})

Tab:AddDropdown({
    Name = "Weapon",
    Default = "Sword",
    Options = {"Sword", "Bow"},
    Callback = function(Value)
        devas_autokill_weapon = Value
    end    
})

local devas_listtt_value = ""

local devas_listtt = Tab:AddParagraph("Connected Instances","nil")

Tab:AddTextbox({
    Name = "GUID",
    Default = "enter entity guid",
    TextDisappear = true,
    Callback = function(Value)
        devas_autokill_add_guid = Value
    end   
})

Tab:AddTextbox({
    Name = "Name",
    Default = "enter entity name",
    TextDisappear = true,
    Callback = function(Value)
        devas_autokill_add_name = Value
    end   
})

Tab:AddButton({
    Name = "Add Instance",
    Callback = function()
        table.insert(devas_autokill_list,{devas_autokill_add_guid,devas_autokill_add_name})
        devas_listtt_value = devas_listtt_value.. "\n".. devas_autokill_add_guid.. " ".. devas_autokill_add_name.. "\n"
        devas_listtt:Set(devas_listtt_value)
    end    
})

local Section = Tab:AddSection({
    Name = "Auto Loot Drop"
})

Tab:AddTextbox({
    Name = "drop name",
    Default = "enter drop name",
    TextDisappear = true,
    Callback = function(Value)
        devas_autoloot_name = Value
    end   
})

Tab:AddTextbox({
    Name = "drop min amount",
    Default = "enter drop min amount",
    TextDisappear = true,
    Callback = function(Value)
        devas_autoloot_min = tonumber(Value)
    end   
})

Tab:AddTextbox({
    Name = "drop max amount",
    Default = "enter drop max amount",
    TextDisappear = true,
    Callback = function(Value)
        devas_autoloot_max = tonumber(Value)
    end   
})

Tab:AddButton({
    Name = "Add AutoLoot to last Instance",
    Callback = function()
        table.insert(devas_autoloot_list,{devas_autokill_add_guid,devas_autoloot_name,devas_autoloot_min,devas_autoloot_max})
    end    
})

Tab:AddToggle({
    Name = "On/Off",
    Default = false,
    Callback = function(Value)
        devas_autokill = Value
        while wait() and devas_autokill == true do
            for i,v in game.Workspace.Game.Regions.Dion.Areas.AncientRuins.MobsSpots:GetChildren() do
                local acces1 = false
                for i,b in devas_autokill_list do
                    if v.Name == b[2] then
                        acces1 = true
                    end
                end

                if acces1 == true then

                    for i,n in v:GetChildren() do
                        local acces2 = false
                        local uuid = ""
                        for i,m in devas_autokill_list do
                            if n.Name == m[2] then
                                acces2 = true
                                uuid = m[1]
                            end
                        end

                        if acces2 == true then
                            local Distance = (n.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude

                            if Distance <= devas_autokill_range then
                                local acces3 = false

                                for i,j in n:GetDescendants() do
                                    if j.ClassName == "Humanoid" then
                                        if j.Health >= 1 then
                                            acces3 = true
                                        end
                                    end
                                end

                                if acces3 == true then
                                    for i,u in devas_autoloot_list do

                                        if u[1] == uuid then
                                            local categor = ""
                                            if u[2] == "Gold" then
                                                categor = "currency"
                                            else
                                                categor = "resources"
                                            end

                                            for i = u[3],u[4] do
                                                wait()

                                                local args = {
                                                    [1] = {
                                                        [categor] = {
                                                            [u[2]] = {
                                                                ["luckActivated"] = false,
                                                                ["amount"] = i
                                                            }
                                                        }
                                                    },
                                                    [2] = {
                                                        [u[1]] = true
                                                    }
                                                }

                                                game:GetService("ReplicatedStorage").Events.Drop.CollectDrop:FireServer(unpack(args))

                                                print(i)
                                            end
                                        end
                                    end

                                else

                                end
                            end
                        end
                    end
                end
            end
        end
    end    
})

local Section = Tab:AddSection({
    Name = "Anti Cheat"
})

Tab:AddButton({
    Name = "Disable Ping Remote",
    Callback = function()
        game.ReplicatedStorage.Events.Player.PingEvent:Destroy()
    end    
})

