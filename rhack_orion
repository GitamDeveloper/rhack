local Section = Tab:AddSection({
	Name = "auto chest farm"
})

Tab:AddToggle({
	Name = "auto farm",
	Default = false,
	Callback = function(Value)
		bf_autocf = Value
		while bf_autocf == true and wait(0.1) do
			if game.Workspace:FindFirstChild("Chest3") then
				if bf_autocf_esp == true then
					getto_closest(game.Workspace, "Chest3", true, Color3.fromRGB(75, 151, 75))
				else
					getto_closest(game.Workspace, "Chest3")
				end
				wait(tweenend_time)

				if bf_autocf_esp == true then
					espp.Enabled = false
				end
			elseif game.Workspace:FindFirstChild("Chest2") then
				if bf_autocf_esp == true then
					getto_closest(game.Workspace, "Chest2", true, Color3.fromRGB(75, 151, 75))
				else
					getto_closest(game.Workspace, "Chest2")
				end
				wait(tweenend_time)

				if bf_autocf_esp == true then
					espp.Enabled = false
				end
			elseif game.Workspace:FindFirstChild("Chest1") then
				if bf_autocf_esp == true then
					getto_closest(game.Workspace, "Chest1", true, Color3.fromRGB(75, 151, 75))
				else
					getto_closest(game.Workspace, "Chest1")
				end
				wait(tweenend_time)

				if bf_autocf_esp == true then
					espp.Enabled = false
				end
			end
		end
	end    
})

Tab:AddToggle({
	Name = "process esp",
	Default = false,
	Callback = function(Value)
		bf_autocf_esp = Value
	end    
})

