		kingl_autof_dist = Value
	end	  
})

local Section = Tab:AddSection({
	Name = "auto stats"
})

Tab:AddToggle({
	Name = "enable",
	Default = false,
	Callback = function(Value)
		kingl_stats = Value
		while kingl_stats == true and wait(0.1) do
			if kingl_stats_Melee == true then
				local args = {
    				[1] = "Melee",
    				[2] = 1
				}

				game:GetService("Players").LocalPlayer.PlayerGui.MainGui.StarterFrame.StatsFrame.RemoteEvent:FireServer(unpack(args))
			end
			if kingl_stats_Defence == true then
				local args = {
    				[1] = "Defence",
    				[2] = 1
				}

				game:GetService("Players").LocalPlayer.PlayerGui.MainGui.StarterFrame.StatsFrame.RemoteEvent:FireServer(unpack(args))
			end
			if kingl_stats_Sword == true then
				local args = {
    				[1] = "Sword",
    				[2] = 1
				}

				game:GetService("Players").LocalPlayer.PlayerGui.MainGui.StarterFrame.StatsFrame.RemoteEvent:FireServer(unpack(args))
			end
			if kingl_stats_Fruit == true then
				local args = {
    				[1] = "Fruit",
    				[2] = 1
				}

				game:GetService("Players").LocalPlayer.PlayerGui.MainGui.StarterFrame.StatsFrame.RemoteEvent:FireServer(unpack(args))
			end
		end
	end    
})

