Tab:AddToggle({
	Name = "death_counter alert",
	Default = false,
	Callback = function(Value)
		tsb_deathcounter_alert = Value
		while tsb_deathcounter_alert == true and wait(0.1) do
			for i,v in game.Workspace.Live:GetDescendants() do
				if v.Name == "Counter" then
					if v.Parent.Name ~= players_gvar.LocalPlayer.Character.Name then
						if v.Parent:FindFirstChild("alert") then
						else
							local alert = Instance.new("Highlight",v.Parent)
	                		alert.FillColor = Color3.fromRGB(252, 3, 3)
                    		alert.Name = "alert"
						end
					end
				else
					if v:FindFirstChild("alert") then
						v:FindFirstChild("alert"):Destroy()
					end
				end
			end
		end
	end    
})

Tab:AddToggle({
	Name = "saitama ult alert",
	Default = false,
	Callback = function(Value)
		tsb_saitama_ult_esp = Value
		while tsb_saitama_ult_esp == true and wait(0.1) do
			for i,v in players_gvar:GetDescendants() do
				if v.Name == "Death Counter" then
					if v.Parent.Parent.Name ~= players_gvar.LocalPlayer.Name then
						if v.Parent.Parent.Character:FindFirstChild("SaitamaUltEsp") then
						else
							apply_text(v.Parent.Parent.Character, "SaitamaUltEsp", "UltUsed", Color3.fromRGB(245, 221, 66))
							Library:MakeNotification({
								Name = "Ultimate alert!",
								Content = "[ ".. v.Parent.Parent.Character.Name.. "] used ult.",
								Image = "",
								Time = 5
							})
						end
					end
				end
			end

			for i,v in game.Workspace.Live:GetDescendants() do
				if v.Name == "SaitamaUltEsp" then

					for o,b in players_gvar:GetChildren() do
						if v.Parent and b then
							if v.Parent.Name == b.Name then
								if b.Backpack:FindFirstChild("Death Counter") then
								else
									v:Destroy()
								end
							end
						end
					end
				end
 			end
		end
	end    
})

