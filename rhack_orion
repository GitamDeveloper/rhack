function noclip()
    Clip = true
    local function Nocl()
        if Clip == true and players_gvar.LocalPlayer.Character ~= nil then
            for _,v in pairs(players_gvar.LocalPlayer.Character:GetDescendants()) do
                if v:IsA('BasePart') and v.CanCollide and v.Name ~= floatName then
                    v.CanCollide = false
                end
            end
        end
        wait(0.21)
    end
    Noclip = game:GetService('RunService').Stepped:Connect(Nocl)
end

function clip()
    if Noclip then Noclip:Disconnect() end
    Clip = false
end

local Section = Tab:AddSection({
	Name = "no clip"
})

Tab:AddToggle({
	Name = "on / off",
	Default = false,
	Callback = function(Value)
		if Value == true then
			noclip()
		end

		if Value == false then
			clip()
		end
	end    
})

local Section = Tab:AddSection({
	Name = "tp to players"
})

Tab:AddButton({
	Name = "tp to player",
	Callback = function()
		if tp_to_mode == "tp" then
			if players_gvar.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
				if players_gvar:FindFirstChild(tpto_plr).Character:FindFirstChild("HumanoidRootPart") then
					players_gvar.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = players_gvar:FindFirstChild(tpto_plr).Character:FindFirstChild("HumanoidRootPart").CFrame
				end
			end
		else
			if tp_to_mode == "tween" then
				if players_gvar.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
					if players_gvar:FindFirstChild(tpto_plr).Character:FindFirstChild("HumanoidRootPart") then
						local tween_goal = {CFrame = players_gvar:FindFirstChild(tpto_plr).Character:FindFirstChild("HumanoidRootPart").CFrame}
						local tween_info = TweenInfo.new(tween_to_time, Enum.EasingStyle.Linear)
						local tween = game:GetService("TweenService"):Create(players_gvar.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"), tween_info, tween_goal)

						tween:Play()
					end
				end
			end
		end
  	end    
})

Tab:AddTextbox({
	Name = "tp to player name",
	Default = "user name",
	TextDisappear = true,
	Callback = function(Value)
		if tp_to_mode == "tp" then
			if players_gvar.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
				if players_gvar:FindFirstChild(Value).Character:FindFirstChild("HumanoidRootPart") then
					players_gvar.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = players_gvar:FindFirstChild(Value).Character:FindFirstChild("HumanoidRootPart").CFrame
				end
			end
		else
			if tp_to_mode == "tween" then
				if players_gvar.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
					if players_gvar:FindFirstChild(Value).Character:FindFirstChild("HumanoidRootPart") then
						local tween_goal = {CFrame = players_gvar:FindFirstChild(Value).Character:FindFirstChild("HumanoidRootPart").CFrame}
						local tween_info = TweenInfo.new(tween_to_time, Enum.EasingStyle.Linear)
						local tween = game:GetService("TweenService"):Create(players_gvar.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"), tween_info, tween_goal)

						tween:Play()
					end
				end
			end
		end

		tpto_plr = Value
	end	  
})

Tab:AddDropdown({
	Name = "mode",
	Default = "tp",
	Options = {"tp","tween"},
	Callback = function(Value)
		tp_to_mode = Value
	end    
})

Tab:AddTextbox({
	Name = "tween time",
	Default = "number",
	TextDisappear = true,
	Callback = function(Value)
		tween_to_time = tonumber(Value)
	end	  
})

local Tab = Window:MakeTab({
	Name = "Exploits",
	Icon = "",
	PremiumOnly = false
})

local Section = Tab:AddSection({
    Name = "Aim"
})

local circlec = Drawing.new("Circle")
circlec.Transparency = 1
circlec.Thickness = 2
circlec.Radius = 180
circlec.Filled = false
circlec.Position = Vector2.new(0,0)
circlec.Color3 = Color3.fromRGB(255,0,0)
circlec.Visible = false

Tab:AddToggle({
    Name = "aim helper",
    Default = false,
    Callback = function(Value)
        aim_helper = Value

        if Value == true then
            circlec.Transparency = 0.5
            circlec.Visible = true
        else
            circlec.Transparency = 1
            circlec.Position = Vector2.new(0,0)
            circlec.Visible = false
        end

        while aim_helper == true and wait() do
            local mouse = players_gvar.LocalPlayer:GetMouse()

            local hit_pos = mouse.Hit.p

            circlec.Position = Vector2.new(mouse.X,mouse.Y)

            for i,v in players_gvar:GetChildren() do
                --wait()

                if v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Head") and v ~= players_gvar.LocalPlayer then

                    local player_pos = v.Character.HumanoidRootPart.Position

                    local _, OnScreen = workspace.CurrentCamera:WorldToScreenPoint(player_pos)

                    --if OnScreen then
                        --if #workspace.CurrentCamera:GetPartsObscuringTarget({workspace.CurrentCamera.CFrame.Position,player_pos},{v.Character.HumanoidRootPart}) == 0 then
                            local dif = (hit_pos - player_pos).Magnitude

                            local pos_player_dif = (players_gvar.LocalPlayer.Character.HumanoidRootPart.Position - player_pos).Magnitude

                            local linen = Drawing.new("Line")
                            linen.Transparency = 1
                            linen.Thickness = 0.5
                            linen.From = Vector2.new(0,0)
                            linen.To = Vector2.new(0,0)
                            --linen.Color3 = Color3.fromRGB(240,255,255)

                            if dif < 0 then
                                dif = dif * -1
                            end

                            if dif == 0 then
                                dif = 1
                            end

                            local player_scr_pos = camera:WorldToScreenPoint(player_pos)

                            local hit_scr_pos = {["X"] = mouse.X, ["Y"] = mouse.Y}

                            local scr_dif_x = player_scr_pos.X - hit_scr_pos.X
                            local scr_dif_y = player_scr_pos.Y - hit_scr_pos.Y

                            if scr_dif_x < 0 then
                                scr_dif_x = scr_dif_x * -1
                            end

                            if scr_dif_x == 0 then
                                scr_dif_x = 1
                            end

                            if scr_dif_y < 0 then
                                scr_dif_y = scr_dif_y * -1
                            end

                            if scr_dif_y == 0 then
                                scr_dif_y = 1
                            end

                            local scr_dif = scr_dif_x + scr_dif_y

                            if scr_dif <= 250 then

                                linen.Transparency = 0.5
                                linen.From = Vector2.new(mouse.X,mouse.Y)
                                linen.To = Vector2.new(player_scr_pos.X,player_scr_pos.Y)

                                local sizez = (pos_player_dif / 50) * (scr_dif / 10)

                                v.Character.Head.CanCollide = false
                                v.Character.Head.Size = Vector3.new(sizez,sizez,sizez)
                                v.Character.Head.Transparency = 0.5
                                v.Character.Head.Color = Color3.fromRGB(255,0,0)

                                v.Character.HumanoidRootPart.CanCollide = false
                                v.Character.HumanoidRootPart.Size = Vector3.new(sizez,sizez,sizez)
                                v.Character.HumanoidRootPart.Transparency = 0.5
                                v.Character.HumanoidRootPart.Color = Color3.fromRGB(255,0,0)

                                --wait()

                            else
                                linen:Remove()

                                v.Character.Head.Size = Vector3.new(1,1,1)
                                v.Character.HumanoidRootPart.Size = Vector3.new(2,4,2)
                            end
                        --end
                    --end
                end
            end
        end
    end    
})

local Section = Tab:AddSection({
    Name = "Char lock"
})

local char_lock_sel = nil
local char_lock_target = nil

Tab:AddBind({
    Name = "char lock",
    Default = Enum.KeyCode.F7,
    Hold = false,
    Callback = function()
        char_lock_sel = nil
        --char_lock_target = nil

        if char_lock == false then
            char_lock = true
        else
            char_lock = false

            if char_lock_target ~= nil and char_lock_target:FindFirstChild("char_lock_esp_target")then
                char_lock_target:FindFirstChild("char_lock_esp_target"):Destroy()
            end
        end

        while char_lock == true and wait() do
            local mouse = players_gvar.LocalPlayer:GetMouse()

            local hit_pos = mouse.Hit.p

            if char_lock_target ~= nil and char_lock_target:FindFirstChild("HumanoidRootPart") then
                local cf = CFrame.lookAt(players_gvar.LocalPlayer.Character.HumanoidRootPart.Position,Vector3.new(char_lock_target.HumanoidRootPart.Position.X,players_gvar.LocalPlayer.Character.HumanoidRootPart.Position.Y,char_lock_target.HumanoidRootPart.Position.Z))
                players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = cf

                if not char_lock_target:FindFirstChild("char_lock_esp_target") then
                    if char_lock == true then
                         print("added target")
                        local high = Instance.new("Highlight",char_lock_target)
                        high.OutlineColor = Color3.fromRGB(200,200,255)
                        high.FillColor = Color3.fromRGB(150,150,205)
                        high.FillTransparency = 0.5
                        high.Name = "char_lock_esp_target"
                    end
                end
            end

            for i,v in players_gvar:GetChildren() do


                if v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Head") and v ~= players_gvar.LocalPlayer then

                    local player_pos = v.Character.HumanoidRootPart.Position

                    local _, OnScreen = workspace.CurrentCamera:WorldToScreenPoint(player_pos)

                    --if OnScreen then
                        --if #workspace.CurrentCamera:GetPartsObscuringTarget({workspace.CurrentCamera.CFrame.Position,player_pos},{v.Character.HumanoidRootPart}) == 0 then
                            local dif = (hit_pos - player_pos).Magnitude

                            local pos_player_dif = (players_gvar.LocalPlayer.Character.HumanoidRootPart.Position - player_pos).Magnitude

                            if dif < 0 then
                                dif = dif * -1
                            end

                            if dif == 0 then
                                dif = 1
                            end

                            local player_scr_pos = camera:WorldToScreenPoint(player_pos)

                            local hit_scr_pos = {["X"] = mouse.X, ["Y"] = mouse.Y}

                            local scr_dif_x = player_scr_pos.X - hit_scr_pos.X
                            local scr_dif_y = player_scr_pos.Y - hit_scr_pos.Y

                            if scr_dif_x < 0 then
                                scr_dif_x = scr_dif_x * -1
                            end

                            if scr_dif_x == 0 then
                                scr_dif_x = 1
                            end

                            if scr_dif_y < 0 then
                                scr_dif_y = scr_dif_y * -1
                            end

                            if scr_dif_y == 0 then
                                scr_dif_y = 1
                            end

                            local scr_dif = scr_dif_x + scr_dif_y

                            if scr_dif <= 50 then

                                char_lock_sel = v.Character

                                if char_lock_sel ~= nil and not char_lock_sel:FindFirstChild("char_lock_esp") then
                                    local high = Instance.new("Highlight",v.Character)
                                    high.OutlineColor = Color3.fromRGB(200,200,255)
                                    high.FillTransparency = 1
                                    high.Name = "char_lock_esp"
                                end
                            else
                                if v.Character:FindFirstChild("char_lock_esp") then
                                    v.Character:FindFirstChild("char_lock_esp"):Destroy()
                                    if v.Character == char_lock_sel then
                                        char_lock_sel = nil
                                    end
                                end
                            end
                        --end
                    --end
                end
            end
        end
    end    
})



Tab:AddBind({
    Name = "select target",
    Default = Enum.KeyCode.F7,
    Hold = false,
    Callback = function()
        if char_lock_sel ~= nil then

            if char_lock_target ~= nil and char_lock_target:FindFirstChild("char_lock_esp_target") then
                char_lock_target:FindFirstChild("char_lock_esp_target"):Destroy()
            end
            char_lock_target = char_lock_sel
        end
    end    
})


local Section = Tab:AddSection({
    Name = "Spin"
})

local fspin_deg = 0

local fspin_x = false
local fspin_y = false
local fspin_z = false

Tab:AddToggle({
    Name = "fast spin",
    Default = false,
    Callback = function(Value)
        fspin = Value

        if Value == true then
            --players_gvar.LocalPlayer.Character.HumanoidRootPart.Anchored = true
        else
            --players_gvar.LocalPlayer.Character.HumanoidRootPart.Anchored = false
        end

        while fspin == true and wait() do
            local humplr = players_gvar.LocalPlayer.Character.HumanoidRootPart

            local spin_x = fspin_deg
            local spin_y = fspin_deg
            local spin_z = fspin_deg

            if fspin_x == false then
                spin_x = 0
            end
            if fspin_y == false then
                spin_y = 0
            end
            if fspin_z == false then
                spin_z = 0
            end

            humplr.CFrame = humplr.CFrame * CFrame.fromEulerAnglesXYZ(spin_x,spin_y,spin_z)
            print("spin")
        end
    end    
})

