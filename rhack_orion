	Name = "Key system"
})

keyt:AddTextbox({
	Name = "key",
	Default = "enter key",
	TextDisappear = true,
	Callback = function(Value)
		if Value == correct_key then
			iskey_correct = true
		else
			for i,v in disablekey_people do
				if v == game:GetService("Players").LocalPlayer.Name then
					iskey_correct = true
				end
			end
		end
	end	  
})
		end
	end
	print("Enter Key In RHACK")
end


