if game.Workspace:FindFirstChild("Rooms") then
	workspace.ChildAdded:Connect(function(obj)
		if pressure_monster_warning == true then
			local can_apply = false
			for i,v in game.ReplicatedStorage.DeathFolder:GetDescendants() do
				if v.Name == obj.Name then
					can_apply = true
				else
				end
			end

			if can_apply == true then
				apply_text(obj, "MonsterTextEsp", "Monster: ".. obj.Name, Color3.fromRGB(250, 5, 5))

            	Library:MakeNotification({
					Name = "Monster alert!",
					Content = "Monster [ ".. obj.Name.. " ] has spawned.",
					Image = "",
					Time = 5
				})

				play_sound(6432593850, 5)

				if pressure_monster_avoid == true then
					local old_pos = players_gvar.LocalPlayer.Character.HumanoidRootPart.Position
					local old_cf = players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame
					local RunServicetp = game:GetService("RunService").Heartbeat:Connect(function()
						players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,1000,0)
						wait(0.1)
						players_gvar.LocalPlayer.Character.HumanoidRootPart.Anchored = true
					end)
					obj.Destroying:Wait()
					RunServicetp:Disconnect()
					wait(0.1)
					players_gvar.LocalPlayer.Character.HumanoidRootPart.Anchored = true
					wait(0.1)
					players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = old_cf
				end
			end
		end

		local can_apply = false
		for i,v in game.ReplicatedStorage.DeathFolder:GetDescendants() do
			if v.Name == obj.Name then
				can_apply = true
			else
			end
		end

		if can_apply == true then
			if pressure_monster_avoid == true then
				local old_pos = players_gvar.LocalPlayer.Character.HumanoidRootPart.Position
				local old_cf = players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame
				local RunServicetp = game:GetService("RunService").Heartbeat:Connect(function()
					players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = (players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame + CFrame.new(0,100000,0))
					--players_gvar.LocalPlayer.Character.HumanoidRootPart.Anchored = true
				end)
				obj.Destroying:Wait()
				RunServicetp:Disconnect()
				players_gvar.LocalPlayer.Character.HumanoidRootPart.Anchored = true

				players_gvar.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(old_cf)
			end
		end
	end)
end

if game.Workspace:FindFirstChild("Rooms") then
	workspace.Rooms.DescendantAdded:Connect(function(obj)
		if obj.Name == "NormalDoor" then
			if pressure_dooresp == true then
				if obj.ClassName == "Model" then

					pressure_doorcount += 1
					local number = 0
					local islock = false
					for i,v in game.Workspace:GetDescendants() do

						if v.Name == "LockerTextEsp" then
							v:Destroy()
						end

						if v.Name == "FakeLockerTextEsp" then
							v:Destroy()
						end

						if v.Name == "RealDoorTextEsp" then
							v:Destroy()
						end

					end

					for i,v in game.Workspace.Rooms:GetChildren() do
						if v.Name ~= "Start" then
							if obj.Parent.Name == "Entrances" then
								number += 1
							end
						end
					end

					for i,v in obj.Parent.Parent:GetDescendants() do
						if v.Name == "KeycardUnlock" then
							islock = true
						end
					end

					if islock == true then
						apply_text(obj, "RealDoorTextEsp", "LockedDoor", Color3.fromRGB(212, 224, 36))

					else
						apply_text(obj, "RealDoorTextEsp", "Door", Color3.fromRGB(14, 168, 12))
					end
				end
			end
		end
	end)
end

