local players_gvar = game:GetService("Players")

local disablekey_people = {"scriptgDev4","scriptgDev3","scriptgDev8","Immortal_YarikDev"}

while iskey_correct == false and wait(0.1) do
	for i,v in disablekey_people do
		if v == game:GetService("Players").LocalPlayer.Name then
			acees_sus = true
		end
	end

	if acees_sus == true then
		iskey_correct = true
	else
		if key_loaded == false then
			key_loaded = true

			local keyw = Library:MakeWindow({Name = "RHACK orion", HidePremium = true, SaveConfig = false, ConfigFolder = "rhack_orion", IntroEnabled = true, IntroText = "RHACK", IntroIcon = "rbxassetid://14995912169", Icon = "rbxassetid://14995912169"})

