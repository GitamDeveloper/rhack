game:GetService("UserInputService").InputBegan:Connect(function(input, isProcessed)
	if ubg_fling == true then
		if input.KeyCode == Enum.KeyCode.One then
			if game.Players.LocalPlayer.Data:FindFirstChild("Character").Value == "Gon" then
				local pos = CFrame.new()
				pos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame

				if ubg_fling_place == "air" then
					local fling_goal = {CFrame = CFrame.new(1000,1000,1000)}
					local fling_info = TweenInfo.new(1, Enum.EasingStyle.Linear)
					local fling = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, fling_info, fling_goal)

					wait(1)

					fling:Play()
				else
					if ubg_fling_place == "castle" then
						local fling_goal = {CFrame = CFrame.new(586.419067, 120.410744, -499.891113, -3.6525809e-14, -4.33599396e-08, -1, -3.07723127e-08, 1, -4.33599396e-08, 1, 3.07723127e-08, -3.78600925e-14)}
						local fling_info = TweenInfo.new(1, Enum.EasingStyle.Linear)
						local fling = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, fling_info, fling_goal)

						wait(1)

						fling:Play()
					end
				end

				wait(3)

				if ubg_fling_back == true then
					local back_goal = {CFrame = pos}
					local back_info = TweenInfo.new(2, Enum.EasingStyle.Linear)
					local back = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, back_info, back_goal)

					back:Play()
				end
			end
		end
	end
end)

