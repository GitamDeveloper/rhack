	Name = "auto parry ball mode",
	Default = "default",
	Options = {"default", "safe tp"},
	Callback = function(Value)
		if Value == "default" then
			bbapbst = false
		end

		if Value == "safe tp" then
			bbapbst = true
		end
	end    
})

