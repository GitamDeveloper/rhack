local invs_clones = Instance.new("Folder",game.Workspace)
invs_clones.Name = "invs_clones"

Tab:AddBind({
	Name = "invisible and god mode",
	Default = Enum.KeyCode.F7,
	Hold = false,
	Callback = function()
		if tsb_invisible == false then
			tsb_invisible = true

			game.Players.LocalPlayer.Character:Clone().Parent = game.Workspace.invs_clones
			game.Players.LocalPlayer.Character.Parent = game.Workspace

			local char = game.Workspace.invs_clones:FindFirstChild(game.Players.LocalPlayer.Name)

			if invs_mode == "tp" then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,2100,0)
			else
				if invs_mode == "tween" then
					local tween_goal = {CFrame = CFrame.new(0,2100,0)}
					local tween_info = TweenInfo.new(invs_time, Enum.EasingStyle.Linear)
					local tween = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"), tween_info, tween_goal)

					tween:Play()
				end
			end

			local speed = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed
			local jump = game.Players.LocalPlayer.Character.Humanoid.JumpPower
			local Maxh = game.Players.LocalPlayer.Character.Humanoid.MaxHealth
			local h = game.Players.LocalPlayer.Character.Humanoid.Health

			game.Workspace.Camera.CameraSubject = char.Humanoid
			game.Players.LocalPlayer.Character = char

			char.Humanoid.WalkSpeed = speed
			char.Humanoid.JumpPower = jump
			char.Humanoid.MaxHealth = Maxh
			char.Humanoid.Health = h
		else
			tsb_invisible = false

			if invs_mode == "tp" then
				game.Workspace:FindFirstChild(game.Players.LocalPlayer.Name).HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
			else
				if invs_mode == "tween" then
					local tween_goal = {CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame}
					local tween_info = TweenInfo.new(invs_time, Enum.EasingStyle.Linear)
					local tween = game:GetService("TweenService"):Create(game.Workspace:FindFirstChild(game.Players.LocalPlayer.Name).HumanoidRootPart.CFrame, tween_info, tween_goal)

					tween:Play()
				end
			end

			game.Players.LocalPlayer.Character = game.Workspace:FindFirstChild(game.Players.LocalPlayer.Name)
			game.Workspace.Camera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
			if game.Workspace.invs_clones:FindFirstChild(game.Players.LocalPlayer.Name) then
				game.Workspace.invs_clones:FindFirstChild(game.Players.LocalPlayer.Name):Destroy()
			end

			game.Workspace:FindFirstChild(game.Players.LocalPlayer.Name).Parent = game.Workspace.invs_clones

			if game.Workspace:FindFirstChild(game.Players.LocalPlayer.Name) then
				game.Workspace:FindFirstChild(game.Players.LocalPlayer.Name):Destroy()
			end
		end
	end    
})

Tab:AddDropdown({
	Name = "move mode",
	Default = "tp",
	Options = {"tp", "tween"},
	Callback = function(Value)
		invs_mode = Value
	end    
})

Tab:AddTextbox({
	Name = "tween time",
	Default = "number",
	TextDisappear = true,
	Callback = function(Value)
		invs_time = tonumber(Value)
	end	  
})

local bug_clones = Instance.new("Folder",game.Workspace)
bug_clones.Name = "bug_clones"

Tab:AddButton({
	Name = "this is for dev (dont press)",
	Callback = function()
		for i,v in game.Workspace.bug_clones:GetDescendants() do
			v:Destroy()
		end


		game.Players.LocalPlayer.Character:Clone().Parent = bug_clones
		game.Players.LocalPlayer.Character.Parent = game.Workspace

		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,2100,0)

		local speed = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed
		local jump = game.Players.LocalPlayer.Character.Humanoid.JumpPower
		local Maxh = game.Players.LocalPlayer.Character.Humanoid.MaxHealth
		local h = game.Players.LocalPlayer.Character.Humanoid.Health

		game.Workspace.Camera.CameraSubject = game.Workspace.bug_clones:FindFirstChild(game.Players.LocalPlayer.Name).Humanoid
		game.Players.LocalPlayer.Character = game.Workspace.bug_clones:FindFirstChild(game.Players.LocalPlayer.Name)

		game.Workspace.bug_clones:FindFirstChild(game.Players.LocalPlayer.Name).Humanoid.WalkSpeed = speed
		game.Workspace.bug_clones:FindFirstChild(game.Players.LocalPlayer.Name).Humanoid.JumpPower = jump
		game.Workspace.bug_clones:FindFirstChild(game.Players.LocalPlayer.Name).Humanoid.MaxHealth = Maxh
		game.Workspace.bug_clones:FindFirstChild(game.Players.LocalPlayer.Name).Humanoid.Health = h

		wait(1)

		print("hhh")
		game.Workspace:FindFirstChild(game.Players.LocalPlayer.Name).HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
		game.Players.LocalPlayer.Character = game.Workspace:FindFirstChild(game.Players.LocalPlayer.Name)
		game.Workspace.Camera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
		if game.Workspace.bug_clones:FindFirstChild(game.Players.LocalPlayer.Name) then
			game.Workspace.bug_clones:FindFirstChild(game.Players.LocalPlayer.Name):Destroy()
		end

		game.Workspace:FindFirstChild(game.Players.LocalPlayer.Name).Parent = game.Workspace.bug_clones

		if game.Workspace:FindFirstChild(game.Players.LocalPlayer.Name) then
			game.Workspace:FindFirstChild(game.Players.LocalPlayer.Name):Destroy()
		end

	end    
})

