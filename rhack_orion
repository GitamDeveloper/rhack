Tab:AddToggle({
	Name = "auto parry lite",
	Default = false,
	Callback = function(Value)
		bb_autoparry = Value
		if Value == true then
			while bb_autoparry == true and wait(bb_autoparry_speed) do
				for _,ball in next, workspace.Balls:GetChildren() do
					if game.Players.LocalPlayer.Character:FindFirstChild("Highlight") then
						local dist = (ball.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
						local vel = ball.AssemblyLinearVelocity.Magnitude

						--local print = dist + vel
						print("dist - ".. tostring(dist).. "parrydist - ".. tostring((vel / bb_autoparry_dist)))
						if (dist) <= (bb_autoparry_dist + vel / bb_autoparry_x) then
							game:GetService("ReplicatedStorage").Remotes.ParryButtonPress:Fire()

							mouse1click()
							print("parry try")
						end
					end
				end
			end
		end
	end    
})

